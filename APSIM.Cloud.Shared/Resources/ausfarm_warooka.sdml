    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Set up the annual livestock management cycle relative to the joining date</val>
    <val>===============================================================================*/</val>
    <val>   !</val>
    <val>define const integer gestation                    = 150                         ! Gestation length for sheep (days)</val>
    <val>define const integer mating_offset                = 12                          ! Average time (days) from start of joining to mating</val>
    <val></val>
    <val>define       integer merino_flush_day</val>
    <val>define       integer merino_late_preg_day</val>
    <val>define       integer merino_births_day</val>
    <val>define       integer merino_castrate_day</val>
    <val>define       integer merino_wean_day</val>
    <val>define       integer merino_replace_age_days</val>
    <val>define       integer merino_join_median</val>
    <val>define       integer merino_scan_day</val>
    <val></val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  /*------------------------------------------------------------------*/</val>
    <val>  /* configure this livestock enterprise(s) from the Params component */</val>
    <val>  set trade_lamb_breed            = F4P_TRADE_BREED</val>
    <val>  set trade_flock_size            = F4P_TRADE_COUNT</val>
    <val>  if trade_flock_size &gt; 0</val>
    <val>     set do_trade_lambs = True    ! turns the trade lambs on/off</val>
    <val>  set trade_lambs_buy_day         = DayOfYear(F4P_TRADE_BUY_ON)</val>
    <val>  set trade_lamb_sale_wt          = F4P_TRADE_LAMB_SALE_WT</val>
    <val>  set merino_ewe_number           = F4P_MERINO_EWES</val>
    <val>  set merino_join_start           = DayOfYear(F4P_MERINO_JOIN)</val>
    <val>  set merino_expected_lambing_rate = F4P_LAMB_RATE</val>
    <val>  set merino_cull_years           = F4P_CULL_YRS</val>
    <val>  set lamb_sale_wt                = F4P_LAMB_SALE_WT</val>
    <val>  set shear_day                   = DayOfYear(F4P_SHEAR_DAY)</val>
    <val>  set  feedlot_feed_1             = F4P_SUPP1</val>
    <val>  set  feedlot_feed_2             = F4P_SUPP2</val>
    <val>  set  feedlot_feed_1_propn       = F4P_SUPP1_PROPN</val>
    <val>  set  feedlot_feed_2_propn       = F4P_SUPP2_PROPN</val>
    <val>  /*------------------------------------------------------------------*/</val>
    <val></val>
    <val>  set merino_join_median                          = 1 + (merino_join_start  + mating_offset               - 1) mod 365</val>
    <val>  set merino_flush_day                            = 1 + (merino_join_start  - flush_length + 365          - 1) mod 365</val>
    <val>  set merino_scan_day                             = 1 + (merino_join_median + gestation div 2             - 1) mod 365</val>
    <val>  set merino_late_preg_day                        = 1 + (merino_join_median + gestation - 30              - 1) mod 365</val>
    <val>  set merino_births_day                           = 1 + (merino_join_median + gestation                   - 1) mod 365</val>
    <val>  set merino_castrate_day                         = 1 + (merino_join_start  + join_length + gestation     - 1) mod 365</val>
    <val>  set merino_wean_day                             = 1 + (merino_join_median + gestation   + wean_age_days - 1) mod 365</val>
    <val></val>
    <val>  set merino_replace_age_days = abs(cull_day - merino_births_day) mod 365    !age of lambs on 1-Nov</val>
    <val>  if merino_replace_age_days &lt; wean_age_days</val>
    <val>  {</val>
    <val>    set merino_replace_age_days = merino_replace_age_days + 365</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Initialise the flock to the current day-of-year and stocking rate</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define real          animals_age</val>
    <val></val>
    <val>define real          merino_survival_rate                                       ! Survival rate of animals (/year)</val>
    <val>define integer       merino_replace_flock_size                                  ! Size of the ewe flock at replacement</val>
    <val>define integer       merino_replace_cohort_size                                 ! Size of the cohort of replacement ewes (number needed to maintain stocking rate)</val>
    <val>define integer       merino_purchase_number                                     ! Number merino ewes to purchase to maintain flock size/stocking rate after cull for age</val>
    <val>define real          merino_years_since_cull                                    ! Number of years since the last replacement event (0-1)</val>
    <val>define integer       merino_flock_size                                          ! Initial ewe flock size, calculated from stocking rate and farm area</val>
    <val>define integer       merino_years_wean_to_cull</val>
    <val>define integer       merino_replace_age_years</val>
    <val>define real          merino_conception_rate</val>
    <val>define integer       merino_days_since_replace</val>
    <val>define integer       merino_days_since_mating</val>
    <val>define integer       merino_days_since_birth</val>
    <val>define integer       merino_days_since_weaning</val>
    <val>define integer       merino_days_since_sale</val>
    <val>define integer       merino_age_years_offset</val>
    <val>define integer       merino_animals_number</val>
    <val>define integer       merino_animals_age</val>
    <val>define text          merino_animals_sex</val>
    <val>define real          merino_animals_weight</val>
    <val>define real          merino_animals_gfw</val>
    <val>define text          merino_animals_paddock</val>
    <val></val>
    <val></val>
    <val>! working variables</val>
    <val>define integer       animals_tag</val>
    <val>define integer       animals_number</val>
    <val>define integer       t</val>
    <val>define integer       existing_groups</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  set merino_survival_rate       = min( 1.0 - animals.genotypes[1]:death_rate, 0.999999 )</val>
    <val>  set merino_years_wean_to_cull  = ((cull_day - merino_wean_day + 365) mod 365) / 365</val>
    <val>  set merino_replace_age_years   = merino_replace_age_days div 365</val>
    <val>  set merino_replace_flock_size  = merino_ewe_number</val>
    <val>  set merino_replace_cohort_size = merino_replace_flock_size * (1.0 - merino_survival_rate) / (1.0 - merino_survival_rate ^ (merino_cull_years - merino_replace_age_years + merino_years_wean_to_cull))</val>
    <val></val>
    <val></val>
    <val>! Compute the expected size of the ewe flock today</val>
    <val>  set merino_years_since_cull    = ((day - cull_day + 365) mod 365) / 365</val>
    <val>  set merino_flock_size          = merino_replace_flock_size * merino_survival_rate ^ merino_years_since_cull</val>
    <val></val>
    <val>! Initial pregnancy rate if mating has already happened</val>
    <val>  set merino_conception_rate     = 0.0</val>
    <val>  for i = 1 to length(animals.genotypes[1]:conception)</val>
    <val>  {</val>
    <val>    set merino_conception_rate    = merino_conception_rate + 0.90 * i * animals.genotypes[1]:conception[i]</val>
    <val>  }</val>
    <val></val>
    <val>! Compute the number of days since various management events</val>
    <val>  set merino_days_since_replace   = (day - cull_day                                        + 730) mod 365</val>
    <val>  set merino_days_since_mating    = (day - (merino_join_start + mating_offset)             + 730) mod 365</val>
    <val>  set merino_days_since_birth     = (day - (merino_join_start + mating_offset + gestation) + 730) mod 365</val>
    <val>  set merino_days_since_weaning   = (day - merino_wean_day                                 + 730) mod 365</val>
    <val>  set merino_days_since_sale      = (day - lamb_sale_first_day                             + 730) mod 365</val>
    <val></val>
    <val>  if (merino_days_since_mating &gt;= gestation)                                    ! Ewes have already given birth</val>
    <val>  {</val>
    <val>    set merino_days_since_mating = 0</val>
    <val>  }</val>
    <val>  if (merino_days_since_weaning &lt; merino_days_since_birth)</val>
    <val>  {</val>
    <val>    set merino_days_since_birth = 0</val>
    <val>  }</val>
    <val></val>
    <val>! Have the animals passed their birthday since the replacement date?</val>
    <val>  if (merino_replace_age_days mod 365 + merino_days_since_replace &gt;= 365)</val>
    <val>  {</val>
    <val>    set merino_age_years_offset = 1</val>
    <val>  }</val>
    <val>  else</val>
    <val>  {</val>
    <val>    set merino_age_years_offset = 0</val>
    <val>  }</val>
    <val></val>
    <val>!-------------------------------------------------------------------------------</val>
    <val>! Set up the ewe flock - Merino</val>
    <val>  {</val>
    <val>     animals.add_animals genotype=merino, number=merino_flock_size, sex='ewe', birth_day=merino_births_day, min_years=merino_age_years_offset, max_years=merino_cull_years+merino_age_years_offset-1, cond_score=3.0, mated_to=merino,  pregnant=merino_days_since_mating, foetuses=merino_conception_rate, lactating=merino_days_since_birth, offspring=merino_expected_lambing_rate</val>
    <val></val>
    <val>! Fix up animals that were too young to be joined at the last joining date      !!!WHAT IS THIS???</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        if (animals.pregnant[i] or animals.lactating[i]) and (animals.age[i] &lt; 365 * merino_join_years + merino_days_since_mating)</val>
    <val>        {</val>
    <val>           set merino_animals_number = animals.number[i]</val>
    <val>           set merino_animals_age    = animals.age[i] * 12.0/365.0</val>
    <val>           set merino_animals_weight = animals.weight[i]</val>
    <val>           set merino_animals_gfw    = animals.fleece_wt[i]</val>
    <val></val>
    <val>           animals.buy  genotype=merino, number=merino_animals_number, sex='ewe', age=merino_animals_age, weight=merino_animals_weight, fleece_wt=merino_animals_gfw</val>
    <val>           animals.sell group=i, number=merino_animals_number</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>!assign tags</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        if (animals.age[i] &lt; wean_age_days+730) and (merino_days_since_mating = 0) and (merino_days_since_birth = 0)</val>
    <val>        {</val>
    <val>           animals.tag group=i, REPLACEMENT_MERINO</val>
    <val>        }</val>
    <val>        else if (animals.age[i] &lt; wean_age_days+730)</val>
    <val>        {</val>
    <val>           animals.tag group=i, EWE_MERINO_MAIDEN</val>
    <val>        }</val>
    <val>        else if (merino_days_since_mating = 0) and (merino_days_since_birth = 0)</val>
    <val>        {</val>
    <val>           animals.tag group=i, EWE_MERINO_DRY</val>
    <val>        }</val>
    <val>        else if (merino_days_since_mating &gt; 0) or (merino_days_since_birth &gt; 0)</val>
    <val>        {</val>
    <val>           animals.tag group=i, EWE_MERINO</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val></val>
    <val>!-------------------------------------------------------------------------------</val>
    <val>! Set up the weaner flock, if any</val>
    <val>     {</val>
    <val>     if (merino_days_since_weaning &lt; merino_days_since_birth) and (merino_days_since_weaning &lt; merino_days_since_sale)</val>
    <val>     {</val>
    <val>        set existing_groups = animals.no_groups</val>
    <val></val>
    <val>        animals.add_animals   genotype=merino, number=0.5*merino_flock_size*merino_expected_lambing_rate, sex='ewe',    birth_day=merino_births_day, min_years=0, max_years=0, cond_score=3.0</val>
    <val></val>
    <val>        if castrate_lambs</val>
    <val>        {</val>
    <val>           animals.add_animals genotype=merino, number=0.5*merino_flock_size*merino_expected_lambing_rate, sex='wether', birth_day=merino_births_day, min_years=0, max_years=0, cond_score=3.0</val>
    <val>        }</val>
    <val>           else</val>
    <val>        {</val>
    <val>            animals.add_animals genotype=merino, number=0.5*merino_flock_size*merino_expected_lambing_rate, sex='ram',    birth_day=merino_births_day, min_years=0, max_years=0, cond_score=3.0</val>
    <val>        }</val>
    <val></val>
    <val>! assign tags to merino lambs</val>
    <val>        for i = existing_groups + 1 to animals.no_groups</val>
    <val>        {</val>
    <val>           if (animals.tag_no[i] = 0) and (animals.sex[i] /= 'ewe')</val>
    <val>           {</val>
    <val>              animals.tag group=i, LAMB_M</val>
    <val>           }</val>
    <val>              else if (animals.tag_no[i] = 0) and (animals.sex[i] = 'ewe')</val>
    <val>           {</val>
    <val>              animals.tag group=i, LAMB_F</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val></val>
    <val>! Make sure that the stock are located on a suitable paddock</val>
    <val>  animals.draft</val>
    <val></val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Subroutine to sell a number of animals, first storing production data</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>subroutine sell_animal_group( group_no : integer; report_tag : integer )</val>
    <val>{</val>
    <val>  define integer     i</val>
    <val>  define integer     t</val>
    <val></val>
    <val>  set i = group_no</val>
    <val></val>
    <val>  call getAnimalClass group_no = i       !determine which age class this group belongs to</val>
    <val></val>
    <val>  if report_tag = 0</val>
    <val>  {</val>
    <val>    set t = animals.tag_no[i]</val>
    <val>  }</val>
    <val>  else</val>
    <val>  {</val>
    <val>    set t = report_tag</val>
    <val>  }</val>
    <val></val>
    <val>  if animals.number[i] &gt; 0</val>
    <val>  {</val>
    <val>    set sale_weight[t]     = (number_sold[t] * sale_weight[t]     + animals.number[i] * animals.weight[i])     / (number_sold[t] + animals.number[i])</val>
    <val>    set sale_cond_score[t] = (number_sold[t] * sale_cond_score[t] + animals.number[i] * animals.cond_score[i]) / (number_sold[t] + animals.number[i])</val>
    <val>    set number_sold[t]     =  number_sold[t]                      + animals.number[i]</val>
    <val></val>
    <val>    set ac_weight_sold[animal_class_found] = (ac_number_sold[animal_class_found] * ac_weight_sold[animal_class_found] + animals.number[i] * animals.weight[i]) / (ac_number_sold[animal_class_found] + animals.number[i])</val>
    <val>    set ac_number_sold[animal_class_found] =  ac_number_sold[animal_class_found] + animals.number[i]</val>
    <val>  }</val>
    <val></val>
    <val></val>
    <val>  animals.sell group=i, number=animals.number[i]</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Subroutine to buy animals, storing production data</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>subroutine buy_animal_group ( group_no : integer; report_tag : integer )</val>
    <val>{</val>
    <val>  define integer     i</val>
    <val>  define integer     t</val>
    <val></val>
    <val>  set i = group_no</val>
    <val></val>
    <val>  call getAnimalClass group_no = i       !determine which age class this group belongs to</val>
    <val></val>
    <val>  if report_tag = 0</val>
    <val>  {</val>
    <val>    set t = animals.tag_no[i]</val>
    <val>  }</val>
    <val>  else</val>
    <val>  {</val>
    <val>    set t = report_tag</val>
    <val>  }</val>
    <val></val>
    <val>  if animals.number[i] &gt; 0</val>
    <val>  {</val>
    <val>    set buy_weight[t]       = (number_purchased[t] * buy_weight[t]     + animals.number[i] * animals.weight[i])     / (number_purchased[t] + animals.number[i])</val>
    <val>    set buy_cond_score[t]   = (number_purchased[t] * buy_cond_score[t] + animals.number[i] * animals.cond_score[i]) / (number_purchased[t] + animals.number[i])</val>
    <val>    set number_purchased[t] =  number_purchased[t]                     + animals.number[i]</val>
    <val></val>
    <val>    set ac_weight_purchased[animal_class_found] = (ac_number_purchased[animal_class_found] * ac_weight_purchased[animal_class_found] + animals.number[i] * animals.weight[i]) / (ac_number_purchased[animal_class_found] + animals.number[i])</val>
    <val>    set ac_number_purchased[animal_class_found] =  ac_number_purchased[animal_class_found] + animals.number[i]</val>
    <val></val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>/* Split animal groups into distinct ages */</val>
    <val>subroutine split_ages ()</val>
    <val>{</val>
    <val>     define integer age_class_yr</val>
    <val>     define integer i</val>
    <val>     for age_class_yr = 1 to 10</val>
    <val>     {</val>
    <val>        for i = 1 to animals.no_groups</val>
    <val>        {</val>
    <val>           animals.split group=i, type='age', value=365*age_class_yr</val>
    <val>        }</val>
    <val>     }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Sale of cull stock</val>
    <val>--------------------------------------------------------------------------------</val>
    <val> The rule used here is to sell down to the desired stocking rate, oldest</val>
    <val> animals first. This will work when stocking rates are being adjusted</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer       tag_counts[NUM_TAGS]</val>
    <val></val>
    <val>define integer       merino_age_class</val>
    <val>define integer       merino_excess_ewes                                         ! Number of ewes to be sold at replaceement</val>
    <val>define integer       merino_cull_ewes</val>
    <val>define integer       merino_no_to_sell</val>
    <val>define integer       merino_no_sold</val>
    <val></val>
    <val></val>
    <val>if day = cull_day</val>
    <val>{</val>
    <val>  set tag_counts         = animals.number_tag                                   ! Compute number of excess ewes</val>
    <val>  set merino_excess_ewes = - merino_replace_flock_size</val>
    <val>  !!!</val>
    <val>  set merino_no_sold    = 0</val>
    <val></val>
    <val>  call split_ages</val>
    <val>  for i = 1 to animals.no_groups</val>
    <val>  {</val>
    <val>     set animals_tag = animals.tag_no[i]</val>
    <val></val>
    <val>     if animals_tag = EWE_MERINO_CULL</val>
    <val>     {</val>
    <val>        set merino_cull_ewes = animals.number[i]</val>
    <val>        call sell_animal_group group_no=i, sale_no=merino_cull_ewes</val>
    <val>        set merino_no_sold = merino_no_sold + merino_cull_ewes</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>!Merino ewes</val>
    <val>  for animals_tag = REPLACEMENT_MERINO to min(EWE_MERINO,length(tag_counts))</val>
    <val>  {</val>
    <val>     set merino_excess_ewes = merino_excess_ewes + tag_counts[animals_tag]</val>
    <val>  }</val>
    <val></val>
    <val>!Split up all age cohorts</val>
    <val>  for merino_age_class = 1 to 10</val>
    <val>  {</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        animals.split group=i, type='age', value=365*merino_age_class</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>  call split_ages</val>
    <val></val>
    <val>!  set merino_no_sold   = 0</val>
    <val>  for merino_age_class = 10 to 1 step -1</val>
    <val>  {</val>
    <val>     for i = 1 to animals.no_groups                                             ! Sell them and record their weight etc</val>
    <val>     {                                                                          ! CHECK - is this selling all old ewes??</val>
    <val>        set merino_animals_number = animals.number[i]</val>
    <val>        set merino_animals_age    = animals.age[i]</val>
    <val></val>
    <val>        if (merino_no_sold &lt; merino_excess_ewes) and (merino_animals_age &gt; 365*merino_age_class) and (merino_animals_age &lt;= 365*(merino_age_class+1))</val>
    <val>        {</val>
    <val>           set merino_no_to_sell = min( merino_animals_number, merino_excess_ewes - merino_no_sold )</val>
    <val>           call sell_animal_group group_no=i, sale_no=merino_no_to_sell</val>
    <val>           set merino_no_sold = merino_no_sold + merino_no_to_sell</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Purchase animals</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer new_group</val>
    <val></val>
    <val>! Purchase trade lambs</val>
    <val>from start</val>
    <val>{</val>
    <val>     if (day =  trade_lambs_buy_day) and (do_trade_lambs = true)</val>
    <val>     {</val>
    <val>        animals.buy genotype=trade_lamb_breed, number=trade_flock_size, sex='wether', age=trade_lambs_buy_age</val>
    <val></val>
    <val>        set new_group = animals.no_groups</val>
    <val>        animals.tag group=new_group, TRADE_LAMBS</val>
    <val></val>
    <val>        if trade_flock_feedlot = TRUE</val>
    <val>        {</val>
    <val>           animals.move group=new_group , paddock= 'farm.trade_feedlot'</val>
    <val>        }</val>
    <val></val>
    <val>        call split_ages</val>
    <val>        !set i = animals.no_groups</val>
    <val>        !call buy_animal_group group_no=new_group, buy_no=animals.number[i]                    ! store info</val>
    <val>        call buy_animal_group group_no=new_group, buy_no=animals.number[new_group]</val>
    <val>     }</val>
    <val>}</val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Reproduction</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer       merino_mate_number</val>
    <val>define integer       merino_no_joined</val>
    <val>define integer       merino_no_to_join</val>
    <val>define integer       merino_no_ewe_groups</val>
    <val>define real          merino_retain_propn</val>
    <val></val>
    <val></val>
    <val>! mate merino ewes to Dohne rams</val>
    <val></val>
    <val>if day = merino_join_start</val>
    <val>{</val>
    <val>  set merino_no_joined = 0</val>
    <val></val>
    <val>  for merino_age_class = merino_join_years to 10</val>
    <val>  {</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        set merino_animals_number = animals.number[i]</val>
    <val>        set merino_animals_age    = animals.age[i]</val>
    <val>        set animals_tag           = animals.tag_no[i]</val>
    <val></val>
    <val>        if (((animals_tag = REPLACEMENT_MERINO) or (animals_tag = EWE_MERINO_MAIDEN_DRY) or (animals_tag = EWE_MERINO_DRY)) and (merino_animals_age &gt; 365*merino_age_class) and (merino_animals_age &lt;= 365*(merino_age_class+1)))</val>
    <val>        {</val>
    <val>           set merino_no_to_join = merino_animals_number</val>
    <val></val>
    <val>           if (animals_tag = EWE_MERINO_DRY)</val>
    <val>           {</val>
    <val>              animals.tag group=i, value=EWE_MERINO</val>
    <val>           }</val>
    <val>           else if (animals_tag = REPLACEMENT_MERINO) or (animals_tag = EWE_MERINO_MAIDEN_DRY)</val>
    <val>           {</val>
    <val>              animals.tag group=i, value=EWE_MERINO_MAIDEN</val>
    <val>           }</val>
    <val></val>
    <val>           animals.join  group=i, mate_to=merino, mate_days=join_length</val>
    <val>           set merino_no_joined = merino_no_joined  + merino_no_to_join</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>if (day = merino_scan_day) and scan_ewes</val>
    <val>{</val>
    <val>  for i = 1 to animals.no_groups</val>
    <val>  {</val>
    <val>     if (animals.tag_no[i] = EWE_MERINO) and (animals.no_foetuses[i] &lt; 1)</val>
    <val>     {</val>
    <val>        animals.tag  group=i, value=EWE_MERINO_CULL</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Castrate male lambs (if required)</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>{</val>
    <val>  if (day = merino_castrate_day) and castrate_lambs</val>
    <val>  {</val>
    <val>     animals.castrate number=99999999</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Weaning</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>! Wean DohneX lambs. Keep some female lambs for DohneX breeding flock</val>
    <val></val>
    <val>define integer       g</val>
    <val>define integer       main_groups</val>
    <val>define integer       new_group_tag</val>
    <val></val>
    <val>{</val>
    <val>  if day = merino_wean_day</val>
    <val>  {</val>
    <val>     set merino_survival_rate       = min( 1.0 - animals.genotypes[1]:death_rate, 0.999999 )</val>
    <val>     set merino_years_wean_to_cull  = ((cull_day - merino_wean_day + 365) mod 365) / 365</val>
    <val>     set merino_replace_age_years   = merino_replace_age_days div 365</val>
    <val>     set merino_replace_flock_size  = merino_ewe_number</val>
    <val>     set merino_replace_cohort_size = merino_replace_flock_size * (1.0 - merino_survival_rate) / (1.0 - merino_survival_rate ^ (merino_cull_years - merino_replace_age_years + merino_years_wean_to_cull))</val>
    <val></val>
    <val>     set main_groups = animals.no_groups                                        ! store the range of original animal groups</val>
    <val>     for g = 1 to main_groups                                                   ! for each animal group</val>
    <val>     {</val>
    <val>        animals_tag = animals.tag_no[g]</val>
    <val>        if (animals_tag = EWE_MERINO) or (animals_tag = EWE_MERINO_MAIDEN)      ! if these merino groups have young</val>
    <val>        {</val>
    <val>           animals.wean group=g, number=animals.number_all                      ! wean the young</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>     for g = main_groups + 1 to animals.no_groups                               ! for each new weaner group</val>
    <val>     {</val>
    <val>        set merino_animals_sex = animals.sex[g]                                 ! determine it's sex</val>
    <val>        set new_group_tag = animals.tag_no[g]</val>
    <val>        if ((new_group_tag = EWE_MERINO) or (new_group_tag = EWE_MERINO_MAIDEN)) and (merino_animals_sex = 'ewe')</val>
    <val>        {</val>
    <val>           animals.tag group=g, value=LAMB_F</val>
    <val>        }</val>
    <val>        else</val>
    <val>        {</val>
    <val>           animals.tag group=g, value=LAMB_M</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>! re-assign tags to ewe mothers to make them dry</val>
    <val>     for g = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        set animals_tag = animals.tag_no[g]</val>
    <val>        if ( (animals_tag = EWE_MERINO_MAIDEN) or (animals_tag = EWE_MERINO) )</val>
    <val>        {</val>
    <val>           animals.tag group=g, value=EWE_MERINO_DRY</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>     if length(animals.number_tag) &gt;= LAMB_F                             ! Identify &amp; tag the replacement ewes</val>
    <val>     {</val>
    <val>        set merino_retain_propn = min( 1.0, merino_replace_cohort_size/(animals.number_tag[LAMB_F] + 0.00000001) )</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set retain_propn = 0.0</val>
    <val>     }</val>
    <val></val>
    <val>     for g = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        if (animals.tag_no[g] = LAMB_F)</val>
    <val>        {</val>
    <val>           animals.split group=g, type='number', value=merino_retain_propn*animals.number[g]</val>
    <val>           animals.tag   group=g, value=REPLACEMENT_MERINO</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Shearing</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer   ac_no_shorn[NUM_CLASSES]</val>
    <val>define real      ac_kg_wool[NUM_CLASSES]</val>
    <val>define real      ac_sum_wool[NUM_CLASSES]</val>
    <val>define real      ac_micron[NUM_CLASSES]</val>
    <val>define real      ac_sum_micron[NUM_CLASSES]</val>
    <val></val>
    <val>define integer   animal_class_found     ! used as a defacto return value from the getAnimalClass() subroutine</val>
    <val></val>
    <val>/*=============================================================================</val>
    <val>   Get the animal class for the specified animal group</val>
    <val>   Uses the animal_class_map[] array defined earlier</val>
    <val>============================================================================= */</val>
    <val>subroutine getAnimalClass( group_no : integer )</val>
    <val>{</val>
    <val>     define integer a_tag</val>
    <val>     define real    a_age</val>
    <val>     define integer class_idx</val>
    <val>     define integer class_tag_idx</val>
    <val></val>
    <val>     set a_tag = animals.tag_no[group_no]</val>
    <val>     set a_age = animals.age[group_no]</val>
    <val></val>
    <val>     animal_class_found = -1</val>
    <val>     set class_idx = 1</val>
    <val>     while (animal_class_found = -1) and (class_idx &lt;= length(animal_class_map))</val>
    <val>     {</val>
    <val>        set class_tag_idx = 1</val>
    <val>        while (animal_class_found = -1) and (class_tag_idx &lt;= length(animal_class_map[class_idx]:tags))</val>
    <val>        {</val>
    <val>           if (a_tag = animal_class_map[class_idx]:tags[class_tag_idx])</val>
    <val>           {</val>
    <val>              if (a_age &gt;= animal_class_map[class_idx]:gte * 365.0) and (a_age &lt; animal_class_map[class_idx]:lt * 365.0)</val>
    <val>                 set animal_class_found = animal_class_map[class_idx]:class</val>
    <val>           }</val>
    <val>           set class_tag_idx = class_tag_idx + 1</val>
    <val>        }</val>
    <val>        set class_idx = class_idx + 1</val>
    <val>     }</val>
    <val>}</val>
    <val></val>
    <val>! --- NUMBER SHORN, KG WOOL, MICRON ---</val>
    <val>! for tags</val>
    <val>if day = shear_day</val>
    <val>{</val>
    <val>  for t = 1 to length(animals.number_tag)                                       ! Store the amount and fibre diameter of the fleeces</val>
    <val>  {</val>
    <val>     set number_shorn[t]    = animals.number_tag[t]</val>
    <val>     set shorn_fleece_wt[t] = animals.fleece_wt_tag[t]                          ! for clean wool set shorn_fleece_wt[t] = animals.c_fleece_wt_tag[t]</val>
    <val>     set shorn_micron[t]    = animals.fibre_diam_tag[t]</val>
    <val>  }</val>
    <val></val>
    <val>  call split_ages</val>
    <val>! for animal classes</val>
    <val>  for i = 1 to animals.no_groups</val>
    <val>  {</val>
    <val>     call getAnimalClass group_no = i       !determine which age class this group belongs to</val>
    <val></val>
    <val>     set ac_no_shorn[animal_class_found]   = ac_no_shorn[animal_class_found]   + animals.number[i]</val>
    <val>     set ac_sum_wool[animal_class_found]   = ac_sum_wool[animal_class_found]   + (animals.number[i] * animals.fleece_wt[i])        ! for clean wool use animals.c_fleece_wt[i]</val>
    <val>     set ac_sum_micron[animal_class_found] = ac_sum_micron[animal_class_found] + (animals.number[i] * animals.fibre_diam[i])</val>
    <val>  } !next animal group</val>
    <val></val>
    <val>  animals.shear                                                                 ! Shear all sheep</val>
    <val></val>
    <val>  ! calculate wool yield kg/head</val>
    <val></val>
    <val>  for i = 1 to NUM_CLASSES</val>
    <val>  {</val>
    <val>     if ac_no_shorn[i] &gt; 0</val>
    <val>     {</val>
    <val>        set ac_kg_wool[i] = ac_sum_wool[i] / ac_no_shorn[i]</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>  ! calculate average wool micron</val>
    <val>  for i = 1 to NUM_CLASSES</val>
    <val>  {</val>
    <val>     if ac_no_shorn[i] &gt; 0</val>
    <val>     {</val>
    <val>        set ac_micron[i] = ac_sum_micron[i] / ac_no_shorn[i]</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>if day = shear_day + 1</val>
    <val>{</val>
    <val>  for t = 1 to length(animals.number_tag)</val>
    <val>  {</val>
    <val>     set number_shorn[t]    = 0</val>
    <val>     set shorn_fleece_wt[t] = 0</val>
    <val>     set shorn_micron[t]    = 0</val>
    <val>  }</val>
    <val></val>
    <val>  for i = 1 to NUM_CLASSES</val>
    <val>  {</val>
    <val>     set ac_no_shorn[i] = 0</val>
    <val>     set ac_sum_wool[i] = 0</val>
    <val>     set ac_sum_micron[i] = 0</val>
    <val>     set ac_kg_wool[i] = 0</val>
    <val>     set ac_micron[i] = 0</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Sale of lambs.</val>
    <val>--------------------------------------------------------------------------------</val>
    <val> There are 2 lamb sale policies considered:</val>
    <val> 1 = sell all lambs at a target weight, or at a final date</val>
    <val> 2 = sell lambs when their rate of weight gain falls below a threshold</val>
    <val></val>
    <val>================================================================================</val>
    <val> Sale policy #1</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define logical       sale_date_range</val>
    <val></val>
    <val></val>
    <val>if (lamb_sale_policy = 1)</val>
    <val>{</val>
    <val>  if lamb_sale_first_day &lt;= lamb_sale_last_day</val>
    <val>  {</val>
    <val>     set sale_date_range = (lamb_sale_first_day &lt;= day) and (day &lt;= lamb_sale_last_day)</val>
    <val>  }</val>
    <val>  else</val>
    <val>  {</val>
    <val>     set sale_date_range = (lamb_sale_first_day &lt;= day) or  (day &lt;= lamb_sale_last_day)</val>
    <val>  }</val>
    <val></val>
    <val>  if sale_date_range</val>
    <val>  {</val>
    <val>     call split_ages</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        if ((animals.tag_no[i] = LAMB_M) or (animals.tag_no[i] = LAMB_F))</val>
    <val>        {</val>
    <val>           if (animals.weight[i] &gt;= lamb_sale_wt) or (day = lamb_sale_last_day)</val>
    <val>           {</val>
    <val>              call sell_animal_group group_no=i, sale_no=animals.number[i]</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Sale policy #2</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer       weaner_count</val>
    <val>define integer       weaner_dwt</val>
    <val></val>
    <val></val>
    <val>if (lamb_sale_policy = 2)</val>
    <val>{</val>
    <val>  set weaner_count = 0</val>
    <val>  set weaner_dwt   = 0.0</val>
    <val>  for i = 1 to animals.no_groups</val>
    <val>  {</val>
    <val>     if ( (animals.tag_no[i] = LAMB_M) or (animals.tag_no[i] = LAMB_F) )</val>
    <val>     {</val>
    <val>        set weaner_count = weaner_count + animals.number[i]</val>
    <val>        set weaner_dwt   = weaner_dwt   + animals.number[i] * animals.wt_change[i]</val>
    <val>     }</val>
    <val>  }</val>
    <val>  if weaner_count &gt; 0</val>
    <val>  {</val>
    <val>     set weaner_dwt = weaner_dwt / weaner_count</val>
    <val>  }</val>
    <val></val>
    <val>  if (weaner_count &gt; 0) and ((weaner_dwt &lt; lamb_sale_dwt) or (day = lamb_sale_last_day))</val>
    <val>  {</val>
    <val>     call split_ages</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        if ( (animals.tag_no[i] = LAMB_M) or (animals.tag_no[i] = LAMB_F))</val>
    <val>        {</val>
    <val>           call sell_animal_group group_no=i, report_tag=animals.tag_no[i]</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> sell trade lambs - after 2 months or when reach target weight</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>if (day = trade_lambs_sell_day) and (do_trade_lambs = true)</val>
    <val>{</val>
    <val>  call split_ages</val>
    <val>  for i = 1 to animals.no_groups</val>
    <val>  {</val>
    <val>     if animals.tag_no[i] = TRADE_LAMBS</val>
    <val>     {</val>
    <val>        if (animals.weight[i] &gt;= trade_lamb_sale_wt) or (day = trade_lambs_sell_day)</val>
    <val>        {</val>
    <val>           call sell_animal_group group_no=i, sale_no=animals.number[i]</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Supplementary feeding</val>
    <val>===============================================================================*/</val>
    <val>for i = 1 to length(supplement.stores)                                          ! Ignore store #1 (the hay store)  - is there a hay store in this sim?</val>
    <val>{</val>
    <val>  if supplement.stores[i]:stored &lt; 20000.0                                      ! Replenish each supplement store</val>
    <val>  {</val>
    <val>     supplement.buy supplement=supplement.stores[i]:name, amount= 100000.0-supplement.stores[i]:stored</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>!-------------------------------------------------------------------------------</val>
    <val></val>
    <val>define integer       merino_feeding_stage = 1</val>
    <val>define logical       feeding[NUM_TAGS]</val>
    <val>define real          feed_rate[NUM_TAGS]</val>
    <val>define real          feed_rate_padd[NUM_TAGS]</val>
    <val>define real          feed_rate_feedlot[NUM_TAGS]</val>
    <val>define real          feed_rate_trade_feedlot[NUM_TAGS]</val>
    <val>define real          low_cs</val>
    <val>define text          feed_paddock</val>
    <val>define real          crit_dwt</val>
    <val>define real          target_wt</val>
    <val>define integer       days_left</val>
    <val></val>
    <val>define real          suppt_fed[SUPPT_COUNT]</val>
    <val>define real          suppt_fed_month_kg[SUPPT_COUNT]</val>
    <val>define real          suppt_fed_month[SUPPT_COUNT]</val>
    <val>define real          suppt_fed_year_kg[SUPPT_COUNT]</val>
    <val>define real          suppt_fed_year[SUPPT_COUNT]</val>
    <val></val>
    <val></val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  for suppt = 1 to SUPPT_COUNT</val>
    <val>  {</val>
    <val>     set suppt_fed[suppt] = 0.0</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>if day_no = 1</val>
    <val>{</val>
    <val>  for suppt = 1 to SUPPT_COUNT</val>
    <val>  {</val>
    <val>     set suppt_fed_month_kg[suppt] = 0.0</val>
    <val>     set suppt_fed[suppt]          = 0.0</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>if day = 1</val>
    <val>{</val>
    <val>  for suppt = 1 to SUPPT_COUNT</val>
    <val>  {</val>
    <val>     set suppt_fed_year_kg[suppt] = 0.0</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>!define feeding stages</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  if day = merino_wean_day                                                      ! Work out which set of condition score</val>
    <val>  {</val>
    <val>     set merino_feeding_stage = 1                                               ! thresholds we should use</val>
    <val>  }</val>
    <val>  if day = merino_flush_day</val>
    <val>  {</val>
    <val>     set merino_feeding_stage = 2</val>
    <val>  }</val>
    <val>  if day = merino_join_start</val>
    <val>  {</val>
    <val>     set merino_feeding_stage = 3</val>
    <val>  }</val>
    <val>  if day = merino_late_preg_day</val>
    <val>  {</val>
    <val>     set merino_feeding_stage = 4</val>
    <val>  }</val>
    <val>  if day = merino_births_day</val>
    <val>  {</val>
    <val>     set merino_feeding_stage = 5</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>define real grp_no</val>
    <val>define text suppt_name_fed</val>
    <val>define real suppt_amount</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  set tag_counts = animals.number_tag                                           ! find the array of tags in the system</val>
    <val>  for t = REPLACEMENT_MERINO to min(TRADE_LAMBS,length(tag_counts))             ! all sheep (TRADE_LAMBS can be last in the list)</val>
    <val>  {</val>
    <val>     set suppt_name_fed = ''</val>
    <val>! find animal in each group with lowest CS</val>
    <val>     set low_cs = 5.0                                                           ! Find the lowest CS in each mob</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        if animals.tag_no[i] = t</val>
    <val>        {</val>
    <val>           set grp_no = i</val>
    <val>           set low_cs = min( low_cs, animals.cond_score[i] )</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>! find animal paddock</val>
    <val>     set feed_paddock = padd_name[1]                                            ! Find the paddock occupied by this mob</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        if animals.tag_no[i] = t</val>
    <val>        {</val>
    <val>           set feed_paddock = animals.paddock[i]</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>! new feedlot feeding rule</val>
    <val></val>
    <val>! merino ewes and lambs</val>
    <val>     if trade_flock_feedlot = TRUE</val>
    <val>     {</val>
    <val>     if ((t &lt; TRADE_LAMBS))</val>
    <val>     {</val>
    <val>        if feed_paddock = FEEDLOT                                               ! This ensures that confinement feeding happens</val>
    <val>        {</val>
    <val>           set feeding[t] = TRUE</val>
    <val>        }</val>
    <val>        else</val>
    <val>        {</val>
    <val>           if not feeding[t] and (low_cs &lt; critical_cs[merino_feeding_stage,t])    ! if no supplement being fed and sheep &lt; critical CS, feed supplement</val>
    <val>           {</val>
    <val>              set feeding[t] = TRUE</val>
    <val>           }</val>
    <val>           else if feeding[t] and (low_cs &gt; critical_cs[merino_feeding_stage,t]+0.1) ! if supplement being fed but sheep &gt; critical CS, stop supplement</val>
    <val>           {</val>
    <val>              set feeding[t] = FALSE</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val></val>
    <val>! trade lambs</val>
    <val>     if (t = TRADE_LAMBS) and (do_trade_lambs = true)</val>
    <val>     {</val>
    <val>        set feeding[t] = TRUE                                                   ! trade lambs always in feedlot being fed</val>
    <val>     }</val>
    <val>     }</val>
    <val></val>
    <val>     else</val>
    <val>     {</val>
    <val>        if feed_paddock = FEEDLOT                                               ! This ensures that confinement feeding happens</val>
    <val>        {</val>
    <val>           set feeding[t] = TRUE</val>
    <val>        }</val>
    <val>        else</val>
    <val>        {</val>
    <val>           if not feeding[t] and (low_cs &lt; critical_cs[merino_feeding_stage,t])    ! if no supplement being fed and sheep &lt; critical CS, feed supplement</val>
    <val>           {</val>
    <val>              set feeding[t] = TRUE</val>
    <val>           }</val>
    <val>           else if feeding[t] and (low_cs &gt; critical_cs[merino_feeding_stage,t]+0.1) ! if supplement being fed but sheep &gt; critical CS, stop supplement</val>
    <val>           {</val>
    <val>              set feeding[t] = FALSE</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>! Vary the feeding rate</val>
    <val>     if feeding[t]</val>
    <val>     {</val>
    <val></val>
    <val>!for merino ewes</val>
    <val>        if (((t = REPLACEMENT_MERINO) or (t = EWE_MERINO_MAIDEN_DRY) or (t = EWE_MERINO_DRY)) and (merino_feeding_stage = 1))  ! Feed ewes up to target condition prior to joining (feeding stage weaning-flushing)</val>
    <val>        {</val>
    <val>           if (day &lt;= merino_join_start)</val>
    <val>           {</val>
    <val>              set days_left = merino_join_start - day + 1</val>
    <val>           }</val>
    <val>           else</val>
    <val>           {</val>
    <val>              set days_left = merino_join_start - day + 366</val>
    <val>           }</val>
    <val>           set target_wt    = animals.genotypes[1]:srw * (1.0 + 0.15 * (critical_cs[merino_feeding_stage,t] - 3.0))</val>
    <val>           set crit_dwt     = (target_wt - animals.base_wt_tag[t]) / days_left</val>
    <val>        }</val>
    <val></val>
    <val></val>
    <val>!for all lambs                                                                  ! Sale weaners are fed to reach target weight</val>
    <val>        else if ( (t = LAMB_M) or (t = LAMB_F) )</val>
    <val>        {</val>
    <val>           if lamb_sale_first_day &lt;= lamb_sale_last_day</val>
    <val>           {</val>
    <val>              set sale_date_range = (lamb_sale_first_day &lt;= day) and (day &lt;= lamb_sale_last_day)</val>
    <val>           }</val>
    <val>           else</val>
    <val>           {</val>
    <val>              set sale_date_range = (lamb_sale_first_day &lt;= day) or  (day &lt;= lamb_sale_last_day)</val>
    <val>           }</val>
    <val></val>
    <val>           if (lamb_sale_policy /= 1) or not sale_date_range</val>
    <val>           {</val>
    <val>              set crit_dwt = 0.05</val>
    <val>           }</val>
    <val>           else</val>
    <val>           {</val>
    <val>           if (day &lt;= lamb_sale_last_day)</val>
    <val>           {</val>
    <val>              set days_left = lamb_sale_last_day - day + 1</val>
    <val>           }</val>
    <val>           else</val>
    <val>           {</val>
    <val>              set days_left = lamb_sale_last_day - day + 366</val>
    <val>           }</val>
    <val>           set crit_dwt    = (lamb_sale_wt - animals.weight_tag[t]) / days_left</val>
    <val>           }</val>
    <val>        }</val>
    <val></val>
    <val>! trade lambs</val>
    <val>        else if (t = TRADE_LAMBS) and (do_trade_lambs = true)</val>
    <val>        {</val>
    <val>           ! these lambs are being sold on a set date after being fed to target weight</val>
    <val>           set crit_dwt = 0</val>
    <val>           set days_left = 0</val>
    <val>           if trade_lambs_buy_day &lt;= trade_lambs_sell_day</val>
    <val>           {</val>
    <val>              if (day &gt;= trade_lambs_buy_day) and (day &lt;= trade_lambs_sell_day)</val>
    <val>              {</val>
    <val>                 set days_left = trade_lambs_sell_day - day + 1</val>
    <val>              }</val>
    <val>           }</val>
    <val>           else                                      ! else buying in month &gt; sell month</val>
    <val>           {</val>
    <val>              if (day &gt;= trade_lambs_buy_day)</val>
    <val>              {</val>
    <val>                 set days_left = trade_lambs_sell_day - day + 366</val>
    <val>              }</val>
    <val>              else if (day &lt;= trade_lambs_sell_day)</val>
    <val>              {</val>
    <val>                 set days_left = trade_lambs_sell_day - day + 1</val>
    <val>              }</val>
    <val>           }</val>
    <val>           if (days_left &gt; 0)</val>
    <val>           {</val>
    <val>              set crit_dwt = (lamb_sale_wt - animals.weight_tag[t]) / days_left</val>
    <val>           }</val>
    <val>        }</val>
    <val>        else</val>
    <val>        {</val>
    <val>           set crit_dwt = 0.00</val>
    <val>        }</val>
    <val></val>
    <val> ! feedlot supplement rule</val>
    <val>        if feed_paddock = FEEDLOT</val>
    <val>        {</val>
    <val>           set suppt_fed_feedlot[t] = 0.0</val>
    <val>           set feed_rate_feedlot[t] = max( 0.0, min( feedlot_max_feed_rate, feed_rate_feedlot[t] - 0.5 * (animals.wt_change_tag[t] - crit_dwt) ) )</val>
    <val>           set suppt_fed_feedlot[t] = animals.number_tag[t] * feed_rate_feedlot[t]</val>
    <val></val>
    <val>           if (feedlot_feed_1_propn &gt; 0.0) and (feedlot_feed_1 /= '')</val>
    <val>           {</val>
    <val>              set suppt_amount = min(1.0, feedlot_feed_1_propn) * suppt_fed_feedlot[t]</val>
    <val>              supplement.feed supplement=feedlot_feed_1, amount=suppt_amount, paddock=feed_paddock</val>
    <val>              call record_supp_fed supplement_name=feedlot_feed_1, suppt_kg=suppt_amount</val>
    <val>           }</val>
    <val>           if (feedlot_feed_2_propn &gt; 0.0) and (feedlot_feed_2 /= '')</val>
    <val>           {</val>
    <val>              set suppt_amount = max(0.0, feedlot_feed_2_propn) * suppt_fed_feedlot[t]</val>
    <val>              supplement.feed supplement=feedlot_feed_2, amount=suppt_amount, paddock=feed_paddock</val>
    <val>              call record_supp_fed supplement_name=feedlot_feed_2, suppt_kg=suppt_amount</val>
    <val>           }</val>
    <val>       }</val>
    <val></val>
    <val>        if feed_paddock = TRADE_FEEDLOT</val>
    <val>        {</val>
    <val>           set suppt_fed_trade_feedlot[t] = 0.0</val>
    <val>           set feed_rate_trade_feedlot[t] = max( 0.0, min( trade_feedlot_max_feed_rate, feed_rate_trade_feedlot[t] - 0.5 * (animals.wt_change_tag[t] - crit_dwt) ) )</val>
    <val>           set suppt_fed_trade_feedlot[t] = animals.number_tag[t] * feed_rate_trade_feedlot[t]</val>
    <val></val>
    <val>           if (trade_feedlot_feed_1_propn &gt; 0.0) and (trade_feedlot_feed_1 /= '')</val>
    <val>           {</val>
    <val>              set suppt_amount = min(1.0, trade_feedlot_feed_1_propn) * suppt_fed_trade_feedlot[t]</val>
    <val>              supplement.feed supplement=trade_feedlot_feed_1, amount=suppt_amount, paddock=feed_paddock</val>
    <val>              call record_supp_fed supplement_name=trade_feedlot_feed_1, suppt_kg=suppt_amount</val>
    <val>           }</val>
    <val>           if (trade_feedlot_feed_2_propn &gt; 0.0) and (trade_feedlot_feed_2 /= '')</val>
    <val>           {</val>
    <val>              set suppt_amount = max(0.0, trade_feedlot_feed_2_propn) * suppt_fed_trade_feedlot[t]</val>
    <val>              supplement.feed supplement=trade_feedlot_feed_2, amount=suppt_amount, paddock=feed_paddock</val>
    <val>              call record_supp_fed supplement_name=trade_feedlot_feed_2, suppt_kg=suppt_amount</val>
    <val>           }</val>
    <val>        }</val>
    <val></val>
    <val>! paddock supplement rule</val>
    <val>        if (feed_paddock /= FEEDLOT) and (feed_paddock /= TRADE_FEEDLOT)</val>
    <val>        {</val>
    <val>           set suppt_fed_padd[t] = 0.0</val>
    <val>           set feed_rate_padd[t] = max( 0.0, min( paddock_max_feed_rate, feed_rate_padd[t] - 0.5 * (animals.wt_change_tag[t] - crit_dwt) ) )</val>
    <val>           set suppt_fed_padd[t] = animals.number_tag[t] * feed_rate_padd[t]</val>
    <val></val>
    <val>           if  (paddock_feed_1_propn &gt; 0.0) and (paddock_feed_1 /= '')</val>
    <val>           {</val>
    <val>              set suppt_amount = min(1.0, paddock_feed_1_propn) * suppt_fed_padd[t]</val>
    <val>              supplement.feed supplement=paddock_feed_1, amount=suppt_amount, paddock=feed_paddock</val>
    <val>              call record_supp_fed supplement_name=paddock_feed_1, suppt_kg=suppt_amount</val>
    <val>           }</val>
    <val>           if (paddock_feed_2_propn &gt; 0.0) and (paddock_feed_2 /= '')</val>
    <val>           {</val>
    <val>              set suppt_amount = max(0.0, paddock_feed_2_propn) * suppt_fed_padd[t]</val>
    <val>              supplement.feed supplement=paddock_feed_2, amount=suppt_amount, paddock=feed_paddock</val>
    <val>              call record_supp_fed supplement_name=paddock_feed_2, suppt_kg=suppt_amount</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }  ! endif feeding[t]</val>
    <val>     else</val>
    <val>     {</val>
    <val>       set feed_rate_padd[t]          = 0.0</val>
    <val>       set feed_rate_feedlot[t]       = 0.0</val>
    <val>       set feed_rate_trade_feedlot[t] = 0.0</val>
    <val>       set suppt_fed_padd[t]          = 0.0</val>
    <val>       set suppt_fed_feedlot[t]       = 0.0</val>
    <val>       set suppt_fed_trade_feedlot[t] = 0.0</val>
    <val>     }</val>
    <val>     ! the total supplement mix fed to animals of this tag</val>
    <val>     set suppt_fed_kg[t] = 0.0</val>
    <val>     set suppt_fed_kg[t] = suppt_fed_padd[t] + suppt_fed_feedlot[t] + suppt_fed_trade_feedlot[t]</val>
    <val>   }  ! next tag</val>
    <val>}</val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Record the supplement ingredient fed</val>
    <val>================================================================================*/</val>
    <val>subroutine record_supp_fed(supplement_name : text; suppt_kg : real )</val>
    <val>{</val>
    <val>     ! accumulate the amount of each supplement type fed</val>
    <val>     set suppt = 1</val>
    <val>     while (suppt &lt;= no_supplements)</val>
    <val>     {</val>
    <val>        if suppt_name[suppt] = supplement_name</val>
    <val>        {</val>
    <val>           set suppt_fed[suppt] = suppt_fed[suppt] + suppt_kg</val>
    <val>           set suppt = no_supplements    ! terminate loop</val>
    <val>        }</val>
    <val>        set suppt = suppt + 1</val>
    <val>     }</val>
    <val>}</val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Monthly reporting of livestock sales, stock inventory</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer   stock_sale_number[NUM_TAGS]</val>
    <val>define real      stock_sale_weight[NUM_TAGS]</val>
    <val>define real      stock_sale_cond_score[NUM_TAGS]</val>
    <val>define integer   stock_sale_age[NUM_TAGS]</val>
    <val>define integer   stock_buy_number[NUM_TAGS]</val>
    <val>define real      stock_buy_weight[NUM_TAGS]</val>
    <val>define real      stock_buy_cond_score[NUM_TAGS]</val>
    <val>define integer   stock_buy_age[NUM_TAGS]</val>
    <val></val>
    <val>define integer   stock_ac_sale_number[NUM_CLASSES]</val>
    <val>define real      stock_ac_sale_weight[NUM_CLASSES]</val>
    <val>define integer   stock_ac_buy_number[NUM_CLASSES]</val>
    <val>define real      stock_ac_buy_weight[NUM_CLASSES]</val>
    <val></val>
    <val>define text      animal_breed[NUM_TAGS]</val>
    <val></val>
    <val>define integer   opening_number[NUM_TAGS]</val>
    <val>define real      opening_weight[NUM_TAGS]</val>
    <val>define real      opening_age[NUM_TAGS]</val>
    <val>define integer   closing_number[NUM_TAGS]</val>
    <val></val>
    <val>define integer   ac_opening_number[NUM_CLASSES]</val>
    <val>define real      ac_opening_weight[NUM_CLASSES]</val>
    <val>define real      ac_opening_age[NUM_CLASSES]</val>
    <val>define real      ac_sum_months[NUM_CLASSES]</val>
    <val>define real      ac_sum_kg[NUM_CLASSES]</val>
    <val>define integer   ac_closing_number[NUM_CLASSES]</val>
    <val></val>
    <val></val>
    <val>define integer last_day[12] = [31,28,31,30,31,30,31,31,30,31,30,31]             !days in month</val>
    <val></val>
    <val>each 1 Jan</val>
    <val>{</val>
    <val>  if year mod 4 = 0</val>
    <val>  {</val>
    <val>    set last_day[2] = 29</val>
    <val>  }</val>
    <val>  else</val>
    <val>  {</val>
    <val>    set last_day[2] = 28</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>! ---- SALE NUMBER, WEIGHT, CS ------</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  if day_no = last_day[month]</val>
    <val>  {</val>
    <val>! store the summary values for the month on the last day of the month</val>
    <val>     set stock_ac_sale_number  = ac_number_sold</val>
    <val>     set stock_ac_sale_weight  = ac_weight_sold</val>
    <val>     set stock_sale_number     = number_sold</val>
    <val>     set stock_sale_weight     = sale_weight</val>
    <val>     set stock_sale_cond_score = sale_cond_score</val>
    <val>     set stock_ac_buy_number   = ac_number_purchased</val>
    <val>     set stock_ac_buy_weight   = ac_weight_purchased</val>
    <val>     set stock_buy_number      = number_purchased</val>
    <val>     set stock_buy_weight      = buy_weight</val>
    <val>     set stock_buy_cond_score  = buy_cond_score</val>
    <val></val>
    <val>  }</val>
    <val>  else</val>
    <val>  {</val>
    <val>     if day_no = 1</val>
    <val>     {</val>
    <val>! initialise the montly working values for each tag</val>
    <val>     for t = 1 to NUM_TAGS</val>
    <val>     {</val>
    <val>        set number_purchased[t]      = 0</val>
    <val>        set buy_weight[t]            = 0.0</val>
    <val>        set buy_cond_score[t]        = 0.0</val>
    <val>        set sale_weight[t]           = 0.0</val>
    <val>        set sale_cond_score[t]       = 0.0</val>
    <val>        set number_sold[t]           = 0</val>
    <val></val>
    <val>        set stock_sale_number[t]     = 0</val>
    <val>        set stock_sale_weight[t]     = 0.0</val>
    <val>        set stock_sale_cond_score[t] = 0.0</val>
    <val>        set stock_buy_number[t]      = 0</val>
    <val>        set stock_buy_weight[t]      = 0.0</val>
    <val>        set stock_buy_cond_score[t]  = 0.0</val>
    <val>     }</val>
    <val>     for t = 1 to NUM_CLASSES</val>
    <val>     {</val>
    <val>        set ac_number_sold[t]        = 0</val>
    <val>        set ac_weight_sold[t]        = 0.0</val>
    <val>        set ac_number_purchased[t]   = 0</val>
    <val>        set ac_weight_purchased[t]   = 0.0</val>
    <val></val>
    <val>        set stock_ac_sale_number[t]  = 0</val>
    <val>        set stock_ac_sale_weight[t]  = 0.0</val>
    <val>        set stock_ac_buy_number[t]   = 0</val>
    <val>        set stock_ac_buy_weight[t]   = 0.0</val>
    <val>     }</val>
    <val></val>
    <val></val>
    <val>    }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>! ---- OPENING COUNT, AGE, WEIGHT ----</val>
    <val></val>
    <val>! for all tags</val>
    <val>from start</val>
    <val>{</val>
    <val>  if day_no = 1</val>
    <val>  {</val>
    <val>     for t = 1 to length(animals.number_tag)</val>
    <val>     {</val>
    <val>        set opening_number[t] = animals.number_tag[t]</val>
    <val>        set opening_weight[t] = animals.weight_tag[t]</val>
    <val>        set opening_age[t]    = animals.age_months_tag[t]</val>
    <val>     }</val>
    <val>  }</val>
    <val>  if day_no = 2</val>
    <val>  {</val>
    <val>     for t = 1 to length(animals.number_tag)</val>
    <val>     {</val>
    <val>        set opening_number[t] = 0</val>
    <val>        set opening_weight[t] = 0</val>
    <val>        set opening_age[t]    = 0</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>define integer tag_index</val>
    <val>define integer current_tag</val>
    <val></val>
    <val>! for animals classes - opening count, weight, age</val>
    <val>from start</val>
    <val>{</val>
    <val>  if day_no = 1</val>
    <val>  {</val>
    <val>     call split_ages</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        call getAnimalClass group_no = i       !determine which age class this group belongs to</val>
    <val></val>
    <val>        set animals_number = animals.number[i]</val>
    <val></val>
    <val>        ! calc and store the monthly opening values for the age classes (other than young)</val>
    <val>        if animal_class_map[animal_class_found]:isyoung = false</val>
    <val>        {</val>
    <val>           set ac_opening_number[animal_class_found] = ac_opening_number[animal_class_found] + animals_number</val>
    <val>           set ac_sum_kg[animal_class_found] = ac_sum_kg[animal_class_found] + (animals.weight[i] * animals_number)</val>
    <val>           set ac_sum_months[animal_class_found] = ac_sum_months[animal_class_found] + (animals.age_months[i] * animals_number)</val>
    <val>        }</val>
    <val>     }  ! next animal group</val>
    <val></val>
    <val>     ! now go through the classes that are suckling young</val>
    <val>     for i = 1 to NUM_CLASSES</val>
    <val>     {</val>
    <val>        if animal_class_map[i]:isyoung = true</val>
    <val>        {</val>
    <val>           for tag_index = 1 to length(animal_class_map[i]:tags)    ! for each tag in this class</val>
    <val>           {</val>
    <val>              set current_tag = animal_class_map[i]:tags[tag_index]</val>
    <val>              set animals_number = animals.number_yng_tag[current_tag]</val>
    <val>              set ac_opening_number[i] = ac_opening_number[i] + animals_number     ! sum the numbers</val>
    <val>              set ac_sum_kg[i] = ac_sum_kg[i] + (animals.weight_yng_tag[current_tag] * animals_number)</val>
    <val>              set ac_sum_months[i] = ac_sum_months[i] + (animals.age_months_yng_tag[current_tag] * animals_number)</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>     for i = 1 to NUM_CLASSES</val>
    <val>     {</val>
    <val>        if ac_opening_number[i] &gt; 0</val>
    <val>        {</val>
    <val>           set ac_opening_weight[i] = ac_sum_kg[i]  / ac_opening_number[i]</val>
    <val>           set ac_opening_age[i]  = ac_sum_months[i]  / ac_opening_number[i]</val>
    <val>        }</val>
    <val>     }</val>
    <val>  } ! endif day = 1</val>
    <val></val>
    <val></val>
    <val></val>
    <val>  if day_no = 2</val>
    <val>  {</val>
    <val>     for i = 1 to NUM_CLASSES</val>
    <val>     {</val>
    <val>        set ac_opening_weight[i] = 0</val>
    <val>        set ac_opening_number[i] = 0</val>
    <val>        set ac_sum_kg[i] = 0</val>
    <val>        set ac_sum_months[i] = 0</val>
    <val>        set ac_opening_age[i] = 0</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>! ---- CLOSING COUNT ----</val>
    <val></val>
    <val>! for all animals</val>
    <val>from start</val>
    <val>{</val>
    <val>  if day_no = last_day[month]</val>
    <val>  {</val>
    <val>     for t = 1 to length(animals.number_tag)</val>
    <val>     {</val>
    <val>        set closing_number[t]             = animals.number_tag[t]</val>
    <val>     }</val>
    <val>  }</val>
    <val>  if day_no = 1</val>
    <val>  {</val>
    <val>     for t = 1 to length(animals.number_tag)</val>
    <val>     {</val>
    <val>        set closing_number[t]            = 0</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>! For classes of animals - closing count</val>
    <val>from start</val>
    <val>{</val>
    <val>  if day_no = last_day[month]</val>
    <val>  {</val>
    <val>     call split_ages</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        call getAnimalClass group_no = i       !determine which age class this group belongs to</val>
    <val></val>
    <val>        if animal_class_map[animal_class_found]:isyoung = false  ! not young</val>
    <val>           set ac_closing_number[animal_class_found] = ac_closing_number[animal_class_found] + animals.number[i]</val>
    <val>     }</val>
    <val></val>
    <val>     ! now go through the classes that are suckling young</val>
    <val>     for i = 1 to NUM_CLASSES</val>
    <val>     {</val>
    <val>        if animal_class_map[i]:isyoung = true</val>
    <val>        {</val>
    <val>           for tag_index = 1 to length(animal_class_map[i]:tags)    ! for each tag in this class</val>
    <val>           {</val>
    <val>              set current_tag = animal_class_map[i]:tags[tag_index]</val>
    <val>              set animals_number = animals.number_yng_tag[current_tag]</val>
    <val>              set ac_closing_number[i] = ac_closing_number[i] + animals_number</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>  if day_no = 1</val>
    <val>  {</val>
    <val>     for i = 1 to NUM_CLASSES</val>
    <val>     {</val>
    <val>        set ac_closing_number[i] = 0</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
