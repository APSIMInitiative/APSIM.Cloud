    <val>  for rot_system = no_rot_systems+1 to no_rot_systems                                       !!</val>
    <val>  {</val>
    <val>     set rot_cover_days[rot_system] = []</val>
    <val>  }</val>
    <val>  set padd_erosion      = paddock_template</val>
    <val>  set rot_erosion       = rot_system_template</val>
    <val>  set padd_n2o_emission = paddock_template</val>
    <val>  set rot_n2o_emission  = rot_system_template</val>
    <val></val>
    <val>  for rot_system = 1 to no_rot_systems</val>
    <val>  {</val>
    <val>     for i = 1 to length(constants.rot_system_info[rot_system]:is_crop)</val>
    <val>     {</val>
    <val>        set pasture_phase[rot_system,i] = not constants.rot_system_info[rot_system]:is_crop[i]</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  set farm_deep_drain      = 0.0</val>
    <val>  set pasture_deep_drain   = 0.0</val>
    <val>  set crop_deep_drain      = 0.0</val>
    <val>  set farm_oc_surface      = 0.0</val>
    <val>  set farm_oc_profile      = 0.0</val>
    <val>  set farm_cover           = 0.0</val>
    <val>  set pasture_cover        = 0.0</val>
    <val>  set crop_cover           = 0.0</val>
    <val>  set farm_erosion         = 0.0</val>
    <val>  set pasture_erosion      = 0.0</val>
    <val>  set crop_erosion         = 0.0</val>
    <val>  set farm_n2o_emission    = 0.0</val>
    <val>  set pasture_n2o_emission = 0.0</val>
    <val>  set crop_n2o_emission    = 0.0</val>
    <val></val>
    <val>  for level = 1 to no_cover_levels</val>
    <val>  {</val>
    <val>     set farm_cover_days[level]       = 0.0</val>
    <val>     set pasture_cover_days[level]    = 0.0</val>
    <val>     set crop_cover_days[level]       = 0.0</val>
    <val>  }</val>
    <val></val>
    <val>  for rot_system = 1 to no_rot_systems</val>
    <val>  {</val>
    <val>     set rot_deep_drain[rot_system]   = 0.0</val>
    <val>     set rot_oc_surface[rot_system]   = 0.0</val>
    <val>     set rot_oc_profile[rot_system]   = 0.0</val>
    <val>     set rot_cover[rot_system]        = 0.0</val>
    <val></val>
    <val>     for level = 1 to no_cover_levels</val>
    <val>     {</val>
    <val>        set rot_cover_days[rot_system,level] = 0.0</val>
    <val>     }</val>
    <val>     set rot_erosion[rot_system]      = 0.0</val>
    <val>     set rot_n2o_emission[rot_system] = 0.0</val>
    <val>  }</val>
    <val></val>
    <val>  set pasture_area  = 0.0</val>
    <val>  set crop_area     = 0.0</val>
    <val></val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     if pasture_phase[padd_to_system[padd], padd_year_no[padd]]</val>
    <val>     {</val>
    <val>        set pasture_area = pasture_area + padd_area[padd]</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set crop_area    = crop_area + padd_area[padd]</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set rot_system      = padd_to_system[padd]</val>
    <val>     set pasture_padd    = pasture_phase[rot_system, padd_year_no[padd]]</val>
    <val>     set rot_area_fract  = padd_area[padd] / system_area[rot_system]</val>
    <val>     set farm_area_fract = padd_area[padd] / farm_area</val>
    <val></val>
    <val>     if pasture_padd</val>
    <val>     {</val>
    <val>        set landuse_area_fract = padd_area[padd] / pasture_area</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set landuse_area_fract = padd_area[padd] / crop_area</val>
    <val>     }</val>
    <val></val>
    <val>     set padd_deep_drain[padd]      = @(water_model[padd] &amp; 'drain')</val>
    <val>     set rot_deep_drain[rot_system] = rot_deep_drain[rot_system] + rot_area_fract * padd_deep_drain[padd]</val>
    <val>     set farm_deep_drain            = farm_deep_drain + farm_area_fract * padd_deep_drain[padd]</val>
    <val></val>
    <val>     if pasture_padd</val>
    <val>     {</val>
    <val>        set pasture_deep_drain       = pasture_deep_drain + landuse_area_fract * padd_deep_drain[padd]</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set crop_deep_drain          = crop_deep_drain + landuse_area_fract * padd_deep_drain[padd]</val>
    <val>     }</val>
    <val></val>
    <val>     set layer_data                 = @(nitrogen_model[padd] &amp; 'carbon_tot')</val>
    <val>     set padd_oc_surface[padd]      = layer_data[1]</val>
    <val>     set rot_oc_surface[rot_system] = rot_oc_surface[rot_system] + rot_area_fract * padd_oc_surface[padd]</val>
    <val>     set farm_oc_surface            = farm_oc_surface + farm_area_fract * padd_oc_surface[padd]</val>
    <val></val>
    <val>     set padd_oc_profile[padd]      = sum(layer_data)</val>
    <val>     set rot_oc_profile[rot_system] = rot_oc_profile[rot_system] + rot_area_fract * padd_oc_profile[padd]</val>
    <val>     set farm_oc_profile            = farm_oc_profile + farm_area_fract * padd_oc_profile[padd]</val>
    <val></val>
    <val>     set padd_cover[padd]           = 1.0 - (1.0 - @(padd_name[padd] &amp; '.cover_tot')) * (1.0 - @(residue_model[padd] &amp; 'surfaceom_cover'))</val>
    <val>     set rot_cover[rot_system]      = rot_cover[rot_system] + rot_area_fract * padd_cover[padd]</val>
    <val>     set farm_cover                 = farm_cover + farm_area_fract * padd_cover[padd]</val>
    <val></val>
    <val>     if pasture_padd</val>
    <val>     {</val>
    <val>        set pasture_cover = pasture_cover + landuse_area_fract * padd_cover[padd]</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set crop_cover  = crop_cover + landuse_area_fract * padd_cover[padd]</val>
    <val>     }</val>
    <val></val>
    <val>     for level = 1 to no_cover_levels</val>
    <val>     {</val>
    <val>        set padd_cover_days[padd,level]      = (padd_cover[padd] &lt; cover_levels[level])</val>
    <val>        set rot_cover_days[rot_system,level] = rot_cover_days[rot_system,level] + rot_area_fract * padd_cover_days[padd,level]</val>
    <val>        set farm_cover_days[level]           = farm_cover_days[level] + farm_area_fract * padd_cover_days[padd,level]</val>
    <val></val>
    <val>        if pasture_padd</val>
    <val>        {</val>
    <val>           set pasture_cover_days[level] = pasture_cover_days[level] + landuse_area_fract * padd_cover_days[padd,level]</val>
    <val>        }</val>
    <val>        else</val>
    <val>        {</val>
    <val>           set crop_cover_days[level] = crop_cover_days[level] + landuse_area_fract * padd_cover_days[padd,level]</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>     set padd_erosion[padd]      = 0.0</val>
    <val>     set rot_erosion[rot_system] = rot_erosion[rot_system] + rot_area_fract * padd_erosion[padd]</val>
    <val>     set farm_erosion            = farm_erosion + farm_area_fract* padd_erosion[padd]</val>
    <val></val>
    <val>     if pasture_padd</val>
    <val>     {</val>
    <val>        set pasture_erosion = pasture_erosion + landuse_area_fract * padd_erosion[padd]</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set crop_erosion  = crop_erosion + landuse_area_fract * padd_erosion[padd]</val>
    <val>     }</val>
    <val></val>
    <val>     set padd_n2o_emission[padd]      = (2*14.0+16.0)/(2*14.0) * sum( @(nitrogen_model[padd] &amp; 'n2o_atm') )   ! units are kg N2O/ha/day</val>
    <val>     set rot_n2o_emission[rot_system] = rot_n2o_emission[rot_system] + rot_area_fract * padd_n2o_emission[padd]</val>
    <val>     set farm_n2o_emission            = farm_n2o_emission + farm_area_fract * padd_n2o_emission[padd]</val>
    <val></val>
    <val>     if pasture_padd</val>
    <val>     {</val>
    <val>        set pasture_n2o_emission      = pasture_n2o_emission + landuse_area_fract * padd_n2o_emission[padd]</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set crop_n2o_emission         = crop_n2o_emission + landuse_area_fract * padd_n2o_emission[padd]</val>
    <val>     }</val>
    <val>  }                                                                             ! loop over paddocks</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Annual soil carbon change, 1 Feb-31 Jan</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define real    padd_delta_oc_surface[PADD_COUNT]</val>
    <val>define real    rot_delta_oc_surface[PADD_COUNT]</val>
    <val>define real    farm_delta_oc_surface</val>
    <val>define real    padd_delta_oc_profile[PADD_COUNT]</val>
    <val>define real    rot_delta_oc_profile[PADD_COUNT]</val>
    <val>define real    farm_delta_oc_profile</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  set padd_delta_oc_surface   = paddock_template</val>
    <val>  set rot_delta_oc_surface    = rot_system_template</val>
    <val>  set padd_delta_oc_profile   = paddock_template</val>
    <val>  set rot_delta_oc_profile    = rot_system_template</val>
    <val>}</val>
    <val></val>
    <val>each 1 Feb</val>
    <val>{</val>
    <val>  set farm_delta_oc_surface = 0.0</val>
    <val>  set farm_delta_oc_profile = 0.0</val>
    <val></val>
    <val>  for rot_system = 1 to no_rot_systems</val>
    <val>  {</val>
    <val>     set rot_delta_oc_surface[rot_system] = 0.0</val>
    <val>     set rot_delta_oc_profile[rot_system] = 0.0</val>
    <val>  }</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set padd_delta_oc_surface[padd] = 0.0</val>
    <val>     set padd_delta_oc_profile[padd] = 0.0</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>each 31 Jan</val>
    <val>{</val>
    <val>  set farm_delta_oc_surface = 0.0</val>
    <val>  set farm_delta_oc_profile = 0.0</val>
    <val>  for rot_system = 1 to no_rot_systems</val>
    <val>  {</val>
    <val>     set rot_delta_oc_surface[rot_system] = 0.0</val>
    <val>     set rot_delta_oc_profile[rot_system] = 0.0</val>
    <val>  }</val>
    <val></val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set rot_system      = padd_to_system[padd]</val>
    <val>     set rot_area_fract  = padd_area[padd] / system_area[rot_system]</val>
    <val>     set farm_area_fract = padd_area[padd] / farm_area</val>
    <val></val>
    <val>     set layer_data                       = @(nitrogen_model[padd] &amp; 'carbon_tot')</val>
    <val>     set padd_delta_oc_surface[padd]      = layer_data[1] - resets_etc.padd_reset_oc[padd,1]</val>
    <val>     set padd_delta_oc_profile[padd]      = sum(layer_data) - sum(resets_etc.padd_reset_oc[padd])</val>
    <val>     set rot_delta_oc_surface[rot_system] = rot_delta_oc_surface[rot_system] + rot_area_fract  * padd_delta_oc_surface[padd]</val>
    <val>     set rot_delta_oc_profile[rot_system] = rot_delta_oc_profile[rot_system] + rot_area_fract  * padd_delta_oc_profile[padd]</val>
    <val>     set farm_delta_oc_surface            = farm_delta_oc_surface + farm_area_fract * padd_delta_oc_surface[padd]</val>
    <val>     set farm_delta_oc_profile            = farm_delta_oc_profile + farm_area_fract * padd_delta_oc_profile[padd]</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Methane emissions per farm hectare (in kg CH4/ha/day)</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define real farm_ch4_emission</val>
    <val></val>
    <val>{</val>
    <val>  set farm_ch4_emission = 0.0</val>
    <val>  for i = 1 to animals.no_groups</val>
    <val>  {</val>
    <val>     set farm_ch4_emission = farm_ch4_emission + animals.number[i] * animals.methane[i] + animals.number_yng[i] * animals.methane_yng[i]</val>
    <val>  }</val>
    <val>  if farm_area &gt; 0</val>
    <val>  {</val>
    <val>     set farm_ch4_emission = farm_ch4_emission / farm_area</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val></val>
  </init>
  <init name="logfile" kind="string">
    <val></val>
  </init>
  <init name="log_set" kind="boolean">
    <val>false</val>
  </init>
  <init name="text" kind="string">
    <val></val>
  </init>
</initsection>
      ]]>
    </initdata>
  </component>

