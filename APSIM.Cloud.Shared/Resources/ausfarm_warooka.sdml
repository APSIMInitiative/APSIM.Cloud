    <val>define string  water_module[PADD_COUNT]</val>
    <val>define string  nitrogen_module[PADD_COUNT]</val>
    <val>define string  nitrogen_model[PADD_COUNT]                        !!!!!!!!!</val>
    <val>define integer no_crop_types</val>
    <val>define string  crop_module[PADD_COUNT,CROP_COUNT]</val>
    <val>define integer no_pasture_types</val>
