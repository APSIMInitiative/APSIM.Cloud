      <y_dm_sen_frac_root>0.005 0.005</y_dm_sen_frac_root>
      <x_dm_sen_frac_leaf description="leaf">0    1     </x_dm_sen_frac_leaf>
      <y_dm_sen_frac_leaf>0    1</y_dm_sen_frac_leaf>
