      <x_stage_code description="stage code used in table">3       4       6       7        9</x_stage_code>
      <y_n_conc_max_leaf description="leaf maximum N conc.">0.0800  0.0650  0.0600  0.0550   0.0350</y_n_conc_max_leaf>
