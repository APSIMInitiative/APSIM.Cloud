    <val>  else if (growing_season_rain &gt; decile8_30aug) and (growing_season_rain &lt;= decile9_30aug)</val>
    <val>  {</val>
    <val>     set raindecile = 9</val>
    <val>  }</val>
    <val>  else if (growing_season_rain &gt; decile9_30aug)</val>
    <val>  {</val>
    <val>     set raindecile = 10</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>each 15 sep</val>
    <val>{</val>
    <val>  if (growing_season_rain &lt;= decile1_15sep)</val>
    <val>  {</val>
    <val>     set raindecile = 1</val>
    <val>  }</val>
    <val>  else if (growing_season_rain &gt; decile1_15sep) and (growing_season_rain &lt;= decile2_15sep)</val>
    <val>  {</val>
    <val>     set raindecile = 2</val>
    <val>  }</val>
    <val>  else if (growing_season_rain &gt; decile2_15sep) and (growing_season_rain &lt;= decile3_15sep)</val>
    <val>  {</val>
    <val>     set raindecile = 3</val>
    <val>  }</val>
    <val>  else if (growing_season_rain &gt; decile3_15sep) and (growing_season_rain &lt;= decile4_15sep)</val>
    <val>  {</val>
    <val>     set raindecile = 4</val>
    <val>  }</val>
    <val>  else if (growing_season_rain &gt; decile4_15sep) and (growing_season_rain &lt;= decile5_15sep)</val>
    <val>  {</val>
    <val>     set raindecile = 5</val>
    <val>  }</val>
    <val>  else if (growing_season_rain &gt; decile5_15sep) and (growing_season_rain &lt;= decile6_15sep)</val>
    <val>  {</val>
    <val>     set raindecile = 6</val>
    <val>  }</val>
    <val>  else if (growing_season_rain &gt; decile6_15sep) and (growing_season_rain &lt;= decile7_15sep)</val>
    <val>  {</val>
    <val>     set raindecile = 7</val>
    <val>  }</val>
    <val>  else if (growing_season_rain &gt; decile7_15sep) and (growing_season_rain &lt;= decile8_15sep)</val>
    <val>  {</val>
    <val>     set raindecile = 8</val>
    <val>  }</val>
    <val>  else if (growing_season_rain &gt; decile8_15sep) and (growing_season_rain &lt;= decile9_15sep)</val>
    <val>  {</val>
    <val>     set raindecile = 9</val>
    <val>  }</val>
    <val>  else if (growing_season_rain &gt; decile9_15sep)</val>
    <val>  {</val>
    <val>     set raindecile = 10</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val></val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> N fertilizer policy for crops</val>
    <val>================================================================================</val>
    <val> * N can be applied at three times:</val>
    <val>   - a given number of days after sowing;</val>
    <val>   - a given day-of-year; or</val>
    <val>   - a given phenological stage.</val>
    <val> * Multiple rules may be specified, in which the case the first rule that is</val>
    <val>   satisfied applies. N application at DAS=1 is a special case, in that</val>
    <val>   subsequent applications are permitted.</val>
    <val> * In all cases, the rate is computed using a function of the form:</val>
    <val>    rate = min_rate + slope * max( 0, reference_day - day )</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define logical crop_topdressed[PADD_COUNT]</val>
    <val></val>
    <val>define integer fert_rule</val>
    <val>define real    crop_das</val>
    <val>define real    crop_phenostage</val>
    <val>define real    crop_zadok</val>
    <val>define real    fert_rate</val>
    <val>define real    fert_no3</val>
    <val>define real    fert_nh4</val>
    <val>define real    fert_urea</val>
    <val>define real    fert_no3_applied[PADD_COUNT]</val>
    <val>define real    fert_nh4_applied[PADD_COUNT]</val>
    <val>define real    fert_urea_applied[PADD_COUNT]</val>
    <val>define real    fert_urea_applied_month[PADD_COUNT]</val>
    <val>define real    N_applied_month[PADD_COUNT]</val>
    <val>define real    N_applied_year[PADD_COUNT]</val>
    <val>define real    total_no3</val>
    <val>define real    total_nh4</val>
    <val>define real    total_urea</val>
    <val>define real    SMN_avail[PADD_COUNT]                      !!?? not in sim</val>
    <val>define real    SMN_layer[PADD_COUNT,10]</val>
    <val>define real    SMN_padd[PADD_COUNT]</val>
    <val>define real    SMN_sum_weighting[PADD_COUNT,10]</val>
    <val>define real    layer_no3[10]</val>
    <val>define real    layer_nh4[10]</val>
    <val></val>
    <val>each 1 Jan</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set padd_n_applied[padd] = 0.0</val>
    <val>     set crop_topdressed[padd] = FALSE</val>
    <val>  }</val>
    <val>  for crop_no = 1 to CROP_COUNT</val>
    <val>  {</val>
    <val>     set crop_n_applied_year[crop_no] = 0.0</val>
    <val>  }</val>
    <val>  set sum_n_year = 0.0</val>
    <val>}</val>
    <val></val>
    <val>each 1 Jan                                                                      ! reset sum fertiliser applied each year</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>      set fert_no3_applied[padd] = 0</val>
    <val>      set fert_nh4_applied[padd] = 0</val>
    <val>      set fert_urea_applied[padd] = 0</val>
    <val>      set fert_urea_applied_month[padd] = 0</val>
    <val>      set N_applied_year[padd] = 0</val>
    <val>      set total_no3 = 0</val>
    <val>      set total_nh4 = 0</val>
    <val>      set total_urea = 0</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>if day_no = 1                                                                   ! reset N applied 1st day each month  - not working!!???</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set fert_urea_applied_month[padd] = 0</val>
    <val>     set N_applied_month[padd] = 0</val>
    <val>  }</val>
    <val>  for crop_no = 1 to CROP_COUNT</val>
    <val>  {</val>
    <val>     set crop_n_applied_month[crop_no] = 0.0</val>
    <val>  }</val>
    <val>  set sum_n_month = 0.0</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>     set fert_no3 = 0</val>
    <val>     set fert_nh4 = 0</val>
    <val>     set fert_urea = 0</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>for padd = 1 to no_paddocks</val>
    <val>{</val>
    <val>  set padd_soil_layer = @(dlayer_variable[padd])</val>
    <val>  set layer_no3       = @(nitrogen_module[padd] &amp; '.model.no3ppm')</val>
    <val>  set layer_nh4       = @(nitrogen_module[padd] &amp; '.model.nh4ppm')</val>
    <val></val>
    <val>  set cumul_depth     = 0.0</val>
    <val>  set SMN_padd[padd]  = 0.0</val>
    <val></val>
    <val>  for layer = 1 to length(padd_soil_layer)</val>
    <val>  {</val>
    <val>     set SMN_layer[padd,layer]         = layer_no3[layer] + layer_nh4[layer]</val>
    <val>     set SMN_sum_weighting[padd,layer] = max( 0.0, min( (SMN_calc_depth_mm - cumul_depth) / padd_soil_layer[layer], 1.0 ) )</val>
    <val>     set cumul_depth                   = cumul_depth + padd_soil_layer[layer]</val>
    <val>     set SMN_padd[padd]                = SMN_padd[padd] + (SMN_sum_weighting[padd,layer] * SMN_layer[padd,layer])</val>
    <val>  }</val>
    <val></val>
    <val></val>
    <val>  set crop_no = crop_sown_no[padd]</val>
    <val>  {</val>
    <val>     if crop_no &gt; 0</val>
    <val>     {</val>
    <val>        if paddock_has_translators[padd]</val>
    <val>        {</val>
    <val>           set crop_model    = crop_module[padd,crop_no] &amp; '.model'</val>
    <val>        }</val>
    <val>        else</val>
    <val>        {</val>
    <val>           set crop_model    = crop_module[padd,crop_no]</val>
    <val>        }</val>
    <val>        set crop_das        = @(crop_model &amp; '.daysaftersowing')</val>
    <val>        set crop_phenostage = @(crop_model &amp; '.stage')</val>
    <val>        !!</val>
    <val></val>
    <val>        if crop_no = 1 or crop_no = 2 or crop_no = 4                            !zadok only for cereals  1=wheat, 2=barley, 4=oats</val>
    <val>        {</val>
    <val>        set crop_zadok      = @(crop_model &amp; '.zadok_stage')</val>
    <val>        }</val>
    <val></val>
    <val>        set fert_rule = 0</val>
    <val>        set rule = 1</val>
    <val>        while (fert_rule = 0) and (rule &lt;= length(crop_fert_info))</val>
    <val>        {</val>
    <val>           set rot_system_matches = (crop_fert_info[rule]:rot_system = padd_system[padd] ) or (crop_fert_info[rule]:rot_system = 'all')</val>
    <val>           set phase_matches      = (crop_fert_info[rule]:phase      = padd_landuse[padd]) or (crop_fert_info[rule]:phase      = 'all')</val>
    <val></val>
    <val>           if (growing_season_rain &gt;= (crop_fert_info[rule]:growing_season_rain)) and (RainDecile &gt;= (crop_fert_info[rule]:RainDecile)) and (avail_soil_water[padd] &gt;= crop_fert_info[rule]:paw_reqd) and (SMN_padd[padd] &lt;= crop_fert_info[rule]:SMN)</val>
    <val>           {</val>
    <val>              if (fert_rule = 0) and rot_system_matches and phase_matches and (crop_fert_info[rule]:cultivar = cultivar_sown[padd])</val>
    <val>              {</val>
    <val>                 if (crop_fert_info[rule]:das = 1) and (crop_das = 1)</val>
    <val>                 {</val>
    <val>                    set fert_rule = rule</val>
    <val>                 }</val>
    <val></val>
    <val>                 else if not crop_topdressed[padd] and (crop_fert_info[rule]:das &gt; 0)   and (crop_das = crop_fert_info[rule]:das)</val>
    <val>                 {</val>
    <val>                    set fert_rule = rule</val>
    <val>                 }</val>
    <val>                 else if not crop_topdressed[padd] and (crop_fert_info[rule]:day &gt; 0)   and (day = crop_fert_info[rule]:day)</val>
    <val>                 {</val>
    <val>                    set fert_rule = rule</val>
    <val>                 }</val>
    <val>                 else if not crop_topdressed[padd] and (crop_fert_info[rule]:stage &gt; 0) and (crop_phenostage &gt;= crop_fert_info[rule]:stage)</val>
    <val>                 {</val>
    <val>                     set fert_rule = rule</val>
    <val>                 }</val>
    <val>                 else if not crop_topdressed[padd] and (padd_landuse[padd] /= 'canola') and (padd_landuse[padd] /= 'chickpea') and (padd_landuse[padd] /= 'lupin') and (padd_landuse[padd] /= 'fieldpea') and (crop_fert_info[rule]:zadok &gt; 0) and (crop_zadok &gt;= crop_fert_info[rule]:zadok)</val>
    <val>                 {</val>
    <val>                     set fert_rule = rule</val>
    <val>                 }</val>
    <val>              }</val>
    <val>           }</val>
    <val></val>
    <val>           set rule = rule + 1</val>
    <val>        }</val>
    <val></val>
    <val></val>
    <val>!!</val>
    <val>        if fert_rule &gt; 0</val>
    <val>        {</val>
    <val>           set fert_rate = crop_fert_info[fert_rule]:base_rate + crop_fert_info[fert_rule]:slope * max( 0.0, crop_fert_info[fert_rule]:base_day - day )</val>
    <val>           set fert_no3  = fert_rate * crop_fert_info[fert_rule]:propn_no3</val>
    <val>           set fert_nh4  = fert_rate * crop_fert_info[fert_rule]:propn_nh4</val>
    <val>           set fert_urea = fert_rate * crop_fert_info[fert_rule]:propn_urea</val>
    <val></val>
    <val>           if paddock_has_translators[padd]</val>
    <val>           {</val>
    <val>              set fert_no3_applied[padd] = fert_no3_applied[padd] + fert_no3 * padd_area[padd]</val>
    <val>              set fert_nh4_applied[padd] = fert_nh4_applied[padd] + fert_nh4 * padd_area[padd]</val>
    <val>              set fert_urea_applied[padd] = fert_urea_applied[padd] + fert_urea * padd_area[padd]</val>
    <val>              set fert_urea_applied_month[padd] = fert_urea_applied_month[padd] + fert_urea * padd_area[padd]</val>
    <val>              set N_applied_month[padd] = fert_urea_applied_month[padd] * 0.46</val>
    <val>              set N_applied_year[padd] = fert_urea_applied[padd] * 0.46</val>
    <val>              set total_no3 = total_no3 +  fert_no3_applied[padd]</val>
    <val>              set total_nh4 = total_nh4 +  fert_nh4_applied[padd]</val>
    <val>              set total_urea = total_urea +  fert_urea_applied[padd]</val>
    <val></val>
    <val>              @(nitrogen_module[padd]).fertilize no3_n=fert_no3, nh4_n=fert_nh4, urea_n=fert_urea</val>
    <val>           }</val>
    <val>           else</val>
    <val>           {</val>
    <val>              if fert_no3 &gt; 0</val>
    <val>              {</val>
    <val>                 set fert_delta[1] = fert_no3</val>
    <val>                 reset @(nitrogen_module[padd] &amp; '.dlt_no3') = fert_delta</val>
    <val>              }</val>
    <val>              if fert_nh4 &gt; 0</val>
    <val>              {</val>
    <val>                 set fert_delta[1] = fert_nh4</val>
    <val>                 reset @(nitrogen_module[padd] &amp; '.dlt_nh4') = fert_delta</val>
    <val>              }</val>
    <val>              if fert_urea &gt; 0</val>
    <val>              {</val>
    <val>                 set fert_delta[1] = fert_urea</val>
    <val>                 reset @(nitrogen_module[padd] &amp; '.dlt_urea') = fert_delta</val>
    <val>              }</val>
    <val>           }</val>
    <val></val>
    <val>           set padd_n_applied[padd] = padd_n_applied[padd] + fert_rate</val>
    <val>           set crop_n_applied_month[crop_no] = crop_n_applied_month[crop_no] + N_applied_month[padd]    ! accumulate the amount for the crop type</val>
    <val>           set crop_n_applied_year[crop_no]  = crop_n_applied_year[crop_no] + N_applied_month[padd]</val>
    <val>           if (crop_das &gt; 1)</val>
    <val>           {</val>
    <val>              set crop_topdressed[padd] = TRUE</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val>set sum_n_month = crop_n_applied_month[1] + crop_n_applied_month[2] + crop_n_applied_month[3] + crop_n_applied_month[4] + crop_n_applied_month[5] + crop_n_applied_month[6]</val>
    <val>set sum_n_year  = crop_n_applied_year[1] + crop_n_applied_year[2] + crop_n_applied_year[3] + crop_n_applied_year[4] + crop_n_applied_year[5] + crop_n_applied_year[6]</val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Pasture fertility scalars are rotation system-, phase- and species-specific</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer rot_system</val>
    <val>define integer fert_year</val>
    <val></val>
    <val>each 1 Jan</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set rot_system = padd_to_system[padd]</val>
    <val>     set fert_year  = min( padd_year_no[padd], length(pasture_fert_scalars[rot_system]) )</val>
    <val>     for pasture_no = 1 to no_pasture_types</val>
    <val>     {</val>
    <val>        if pasture_module[padd,pasture_no] /= ''</val>
    <val>        {</val>
    <val>           reset @(pasture_module[padd,pasture_no] &amp; '.fertility') = pasture_fert_scalars[rot_system,fert_year,pasture_no]</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Sow pastures on the same running rainfall total as crops</val>
    <val> * Each pasture species in the sward has its own sowing rule</val>
    <val> * (Re-) sowing can be suppressed if the current root mass of a species is</val>
    <val>   above a threshold</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define logical pasture_in[PADD_COUNT,PAST_COUNT]</val>
    <val></val>
    <val>define real    pasture_root_dm</val>
    <val>define logical species_matches</val>
    <val>define integer other_pasture</val>
    <val></val>
    <val>each 1 Jan</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     for pasture_no = 1 to no_pasture_types</val>
    <val>     {</val>
    <val>        set pasture_in[padd,pasture_no] = FALSE</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     for pasture_no = 1 to no_pasture_types</val>
    <val>     {</val>
    <val>        if (pasture_module[padd,pasture_no] /= '') and not pasture_in[padd,pasture_no]</val>
    <val>        {</val>
    <val>           set pasture_root_dm = @( pasture_module[padd,pasture_no] &amp; '.root_dm' )</val>
    <val></val>
    <val>           set sow_rule = 0                                                     ! Is there a sowing rule that applies to this paddock &amp; pasture species today?</val>
    <val>           for rule = 1 to length(pasture_sowing_info)</val>
    <val>           {</val>
    <val>              set rot_system_matches = (pasture_sowing_info[rule]:rot_system = padd_system[padd] ) or (pasture_sowing_info[rule]:rot_system = 'all')</val>
    <val>              set phase_matches      = (pasture_sowing_info[rule]:phase      = padd_landuse[padd]) or (pasture_sowing_info[rule]:phase      = 'all')</val>
    <val>              set species_matches    = (pasture_sowing_info[rule]:pasture    = pasture_name[pasture_no])</val>
    <val>              if (sow_rule = 0) and rot_system_matches and phase_matches and species_matches</val>
    <val>              {</val>
    <val>                 if (day &gt;= pasture_sowing_info[rule]:first_day) and (day &lt;= pasture_sowing_info[rule]:last_day) and (cumul_rain &gt;= pasture_sowing_info[rule]:rain_reqd) !and (pasture_root_dm &lt; pasture_sowing_info[rule]:resow_root_dm)</val>
    <val>                 {</val>
    <val>                    set sow_rule = rule</val>
    <val>                 }</val>
    <val>                 else if (day = pasture_sowing_info[rule]:last_day) and pasture_sowing_info[rule]:sow_dry</val>
    <val>                 {</val>
    <val>                    set sow_rule = rule</val>
    <val>                 }</val>
    <val>              }</val>
    <val>           }</val>
    <val></val>
    <val>! Carry out the sowing actions</val>
    <val>           if sow_rule &gt; 0</val>
    <val>           {</val>
    <val>              if pasture_sowing_info[sow_rule]:spray                            ! If called for, spray out pastures at sowing</val>
    <val>              {</val>
    <val>                 for other_pasture = 1 to no_pasture_types</val>
    <val>                 {</val>
    <val>                    if (pasture_module[padd,other_pasture] /= '')</val>
    <val>                    {</val>
    <val>                       @(pasture_module[padd,other_pasture]).kill propn_herbage=1.0, propn_seed=0.0</val>
    <val>                    }</val>
    <val>                 }</val>
    <val>              }</val>
    <val></val>
    <val>              @(pasture_module[padd,pasture_no]).sow rate=pasture_sowing_info[sow_rule]:rate</val>
    <val></val>
    <val>              set pasture_in[padd,pasture_no] = TRUE</val>
    <val>              set area_sown[padd]             = padd_area[padd]</val>
    <val></val>
    <val>              if crop_sown[padd] = '-'</val>
    <val>              {</val>
    <val>                 set crop_sown[padd]  = 'pasture'</val>
    <val>              }</val>
    <val>              else</val>
    <val>              {</val>
    <val>                 set crop_sown[padd]  = crop_sown[padd] &amp; str('+') &amp; 'pasture'</val>
    <val>              }</val>
    <val>              if cultivar_sown[padd] = '-'</val>
    <val>              {</val>
    <val>                 set cultivar_sown[padd] = pasture_name[pasture_no]</val>
    <val>              }</val>
    <val>              else</val>
    <val>              {</val>
    <val>                 set cultivar_sown[padd] = cultivar_sown[padd] &amp; str('+') &amp; pasture_name[pasture_no]</val>
    <val>              }</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val></val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Herbicide applications to pastures</val>
    <val> * Two kinds of application rule are handled here:</val>
    <val>   - spraying all "weed" species when green mass of weeds exceeds a threshold</val>
    <val>   - spraying one (or all) species at a particular day of year within a</val>
    <val>     rotation phase</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define real    green_weed_mass                                                  ! kg/ha</val>
    <val>define logical in_period</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     for rule = 1 to length(pasture_kill_info)</val>
    <val>     {</val>
    <val>        set rot_system_matches = (pasture_kill_info[rule]:rot_system = padd_system[padd] ) or (pasture_kill_info[rule]:rot_system = 'all')</val>
    <val>        set phase_matches      = (pasture_kill_info[rule]:phase      = padd_landuse[padd]) or (pasture_kill_info[rule]:phase      = 'all')</val>
    <val></val>
    <val>        if rot_system_matches and phase_matches and (pasture_kill_info[rule]:weed_mass &gt; 0.0)</val>
    <val>        {</val>
    <val>           set green_weed_mass = 0.0</val>
    <val>           for pasture_no = 1 to no_pasture_types</val>
    <val>           {</val>
    <val>              if (pasture_module[padd,pasture_no] /= '') and is_weed_species[pasture_no]</val>
    <val>              {</val>
    <val>                 set green_weed_mass = green_weed_mass + @(pasture_module[padd,pasture_no] &amp; '.green_dm')</val>
    <val>              }</val>
    <val>           }</val>
    <val></val>
    <val>           if pasture_kill_info[rule]:first_day &lt;= pasture_kill_info[rule]:last_day</val>
    <val>           {</val>
    <val>              set in_period = (pasture_kill_info[rule]:first_day &lt;= day) and (day &lt;= pasture_kill_info[rule]:last_day)</val>
    <val>           }</val>
    <val>           else</val>
    <val>           {</val>
    <val>              set in_period = (pasture_kill_info[rule]:first_day &lt;= day) or  (day &lt;= pasture_kill_info[rule]:last_day)</val>
    <val>           }</val>
    <val></val>
    <val>           if in_period and (green_weed_mass &gt; pasture_kill_info[rule]:weed_mass)</val>
    <val>           {</val>
    <val>              for pasture_no = 1 to no_pasture_types</val>
    <val>              {</val>
    <val>                 if (pasture_module[padd,pasture_no] /= '') and is_weed_species[pasture_no]</val>
    <val>                 {</val>
    <val>                    @(pasture_module[padd,pasture_no]).kill propn_herbage=pasture_kill_info[rule]:killed, propn_seed=0.0</val>
    <val>                 }</val>
    <val>              }</val>
    <val>           }</val>
    <val>        }</val>
    <val></val>
    <val>        else if rot_system_matches and phase_matches and (day = pasture_kill_info[rule]:first_day)</val>
    <val>        {</val>
    <val>           for pasture_no = 1 to no_pasture_types</val>
    <val>           {</val>
    <val>              if (pasture_module[padd,pasture_no] /= '') and ((pasture_kill_info[rule]:pasture = pasture_name[pasture_no]) or (pasture_kill_info[rule]:pasture = 'all'))</val>
    <val>              {</val>
    <val>                 @(pasture_module[padd,pasture_no]).kill propn_herbage=pasture_kill_info[rule]:killed, propn_seed=0.0</val>
    <val>              }</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
  </init>
  <init name="logfile" kind="string">
    <val></val>
  </init>
  <init name="log_set" kind="boolean">
    <val>false</val>
  </init>
  <init name="text" kind="string">
    <val></val>
  </init>
</initsection>
      ]]>
    </initdata>
  </component>

  <component name="grazing" active="T" class="Manager">
    <executable name="manager.dll" version="1.1"/>
    <initdata>
      <![CDATA[
<initsection>
  <init name="published_events" array="T">
  </init>
  <init name="subscribed_events" array="T">
  </init>
  <init name="driver_connections" array="T">
  </init>
  <init name="rules" kind="string" array="T">
    <val>/*==============================================================================</val>
    <val> Grazing management</val>
    <val>===============================================================================</val>
    <val> Short-interval, flexible grazing is used.</val>
    <val> The grazing schedule is divided into 4 stages:</val>
    <val>   Starts             Stubbles       Crops         Other paddocks    Feedlot</val>
    <val>   --------------     ----------     --------      --------------    ---------</val>
    <val> 1 Stubbles avail.    mature ewes    -             Weaners, maidens  -</val>
    <val> 2 End of stubbles      -            -             All stock         -</val>
    <val> 3 Oats                 -            all stock     -                 -</val>
    <val> 4 Cover too low        -            -             -                 All stock</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define const integer PADD_COUNT           = 21                                  ! from constants</val>
    <val>define const integer ROT_COUNT            = 3</val>
    <val>define const integer CROP_COUNT           = 8</val>
    <val>define const integer PAST_COUNT           = 1</val>
    <val></val>
    <val>! GRAZING STAGES                                                                ! Numeric values used to denote "stages" in the grazing management logic</val>
    <val>define const integer GRAZE_STUBBLE        = 1</val>
    <val>define const integer GRAZE_GENERAL        = 2</val>
    <val>define const integer GRAZE_CROP           = 3                                   ! Graze oats and fieldpea</val>
    <val>define const integer GRAZE_CONFINE        = 4</val>
    <val></val>
    <val>define text    FEEDLOT                                                          ! set from the constants script</val>
    <val>define text    TRADE_FEEDLOT</val>
    <val>define text    maint_feed_location        = 'feedlot'                           ! feed animals in this location</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Tag values - must match "livestock" script</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define const integer NUM_TAGS                     = 9                          ! ** number of tag groups defined below **</val>
    <val></val>
    <val>define const integer REPLACEMENT_MERINO           = 1</val>
    <val>define const integer EWE_MERINO_MAIDEN_DRY        = 2</val>
    <val>define const integer EWE_MERINO_MAIDEN            = 3</val>
    <val>define const integer EWE_MERINO_DRY               = 4</val>
    <val>define const integer EWE_MERINO                   = 5</val>
    <val>define const integer EWE_MERINO_CULL              = 6</val>
    <val>define const integer LAMB_M                       = 7</val>
    <val>define const integer LAMB_F                       = 8</val>
    <val>define const integer TRADE_LAMBS                  = 9                      !</val>
    <val></val>
    <val></val>
    <val>define integer padd</val>
    <val>define integer i</val>
    <val>define integer t</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Copy constants across from other modules</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer no_paddocks</val>
    <val>define text    padd_name[PADD_COUNT]</val>
    <val>define text    padd_system[PADD_COUNT]</val>
    <val>define integer no_crop_types</val>
    <val>define text    crop_name[CROP_COUNT]</val>
    <val>define text    crop_module[PADD_COUNT,CROP_COUNT]</val>
    <val>define integer no_pasture_types</val>
    <val>define text    pasture_module[PADD_COUNT,PAST_COUNT]</val>
    <val>define integer max_stubble_days</val>
    <val>define real    stubble_cover_threshold</val>
    <val>define real    stubble_green_pasture_threshold</val>
    <val>define real    pasture_cover_threshold</val>
    <val>define real    pasture_dm_results[PADD_COUNT,2,PAST_COUNT]                               ! paddocks x green/dry x pasture</val>
    <val>!define logical crop_in[PADD_COUNT]</val>
    <val></val>
    <val></val>
    <val>define         crop_grazing_info               = [ (rotation:''; phase:''; cultivar:''; initial_dm:0.0; final_dm:0.0; final_stage:0; dse_days:0.0)]</val>
    <val></val>
    <val></val>
    <val></val>
    <val>! TAGS                                            ! 1        2       3       4       5       6       7       8       9</val>
    <val>define integer grazing_priorities[NUM_TAGS]    =   [ 2,      2,      2,      2,      2,      2,      1,      1 ,     1]</val>
    <val></val>
    <val></val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  set max_stubble_days                = constants.max_stubble_days</val>
    <val>  set stubble_cover_threshold         = constants.stubble_cover_threshold</val>
    <val>  set stubble_green_pasture_threshold = constants.stubble_green_pasture_threshold</val>
    <val>  set pasture_cover_threshold         = constants.pasture_cover_threshold</val>
    <val>  set no_paddocks                     = constants.no_paddocks</val>
    <val>  set padd_name                       = constants.padd_name</val>
    <val>  set padd_system                     = constants.padd_system</val>
    <val>  set no_crop_types                   = constants.no_crop_types</val>
    <val>  set crop_name                       = constants.crop_name</val>
    <val>  set crop_module                     = constants.crop_module</val>
    <val>  set no_pasture_types                = constants.no_pasture_types</val>
    <val>  set pasture_module                  = constants.pasture_module</val>
    <val>  set crop_grazing_info               = constants.crop_grazing_info</val>
    <val>  set FEEDLOT                         = constants.FEEDLOT</val>
    <val>  set TRADE_FEEDLOT                   = constants.TRADE_FEEDLOT</val>
    <val>  set pasture_dm_results              = summaries.pasture_dm_results</val>
    <val>  set trade_flock_feedlot             = livestock.trade_flock_feedlot</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Production summary variables that are computed at events</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define text    paddock_occupied[NUM_TAGS]</val>
    <val>define text    not_a_paddock[NUM_TAGS]    ! used to reset the paddock_occupied</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>     for t = 1 to NUM_TAGS</val>
    <val>     {</val>
    <val>        not_a_paddock[t] = '-'</val>
    <val>     }</val>
    <val>}</val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> These rules manage the shifts of grazing regime from stage to stage</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer grazing_stage        = 2                                         ! initial grazing stage??</val>
    <val>define integer stubble_days         = 0                                         ! what does this mean? is it min/max days grazing stubble?</val>
    <val>define real    lowest_pasture_cover = 1.0</val>
    <val></val>
    <val>define logical stubble_present[PADD_COUNT]</val>
    <val>define real    best_stubble_cover</val>
    <val>define real    padd_stubble_cover</val>
    <val>define real    best_green_pasture_dm</val>
    <val></val>
    <val>define real    spilt_grain_remaining[PADD_COUNT]</val>
    <val>define text    spilt_grain_name[PADD_COUNT]</val>
    <val>define integer spilt_grain_id[PADD_COUNT]</val>
    <val></val>
    <val>define logical do_confinement_feeding</val>
    <val>define integer crop_no</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Separate pasture from crop paddocks</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define logical is_pasture_paddock[PADD_COUNT]</val>
    <val>define logical is_cropping_paddock[PADD_COUNT]</val>
    <val></val>
    <val></val>
    <val>/*each 2 jan</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set is_pasture_paddock[padd]  = (padd_area[padd] &gt; 0.005*constants.farm_area) and not rot_system_info[ constants.padd_to_system[padd] ]:is_crop[ padd_year_no[padd] ]</val>
    <val>     set is_cropping_paddock[padd] = (padd_area[padd] &gt; 0.005*constants.farm_area) and     rot_system_info[ constants.padd_to_system[padd] ]:is_crop[ padd_year_no[padd] ]</val>
    <val>  }</val>
    <val>}    */</val>
    <val></val>
    <val></val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set is_pasture_paddock[padd]  = (padd_area[padd] &gt; 0.005*constants.farm_area) and not cropping.crop_in[padd]</val>
    <val>     set is_cropping_paddock[padd] = (padd_area[padd] &gt; 0.005*constants.farm_area) and     cropping.crop_in[padd]</val>
    <val>  }</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Stubble grazing</val>
    <val> * During October-January, detect the availability of stubbles and hence the</val>
    <val>   start of grazing stage 1</val>
    <val> * End stubble grazing after a fixed time, or when there is</val>
    <val>   insufficient stubble cover</val>
    <val> * The spilt grain "submodel" is initialized here</val>
    <val>================================================================================*/</val>
    <val></val>
    <val></val>
    <val></val>
    <val>from 1 Oct to 1 Apr</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>        for crop_no = 1 to no_crop_types</val>
    <val>        {</val>
    <val>           if cropping.crop_harvested[padd,crop_no] or cropping.crop_cut[padd,crop_no]</val>
    <val>           {</val>
    <val>           set stubble_present[padd]       = TRUE</val>
    <val>           set spilt_grain_remaining[padd] = cropping.grain_spilt_today[padd]</val>
    <val>           set spilt_grain_name[padd]      = cropping.grain_spilt_name[padd]</val>
    <val>           set spilt_grain_id[padd]        = crop_no</val>
    <val>           }</val>
    <val>        }</val>
    <val>  }</val>
    <val></val>
    <val>  if ( (grazing_stage = GRAZE_GENERAL) or (grazing_stage = GRAZE_CONFINE) or (grazing_stage = GRAZE_CROP) ) and (max_stubble_days &gt; 0)     ! move animals onto stubble paddocks</val>
    <val>  {</val>
    <val>     for padd = 1 to no_paddocks</val>
    <val>     {</val>
    <val>        if (stubble_present[padd])</val>
    <val>        {</val>
    <val>           set grazing_stage = GRAZE_STUBBLE</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>from 1 Oct to 1 Apr                                                            ! Stage 1 -&gt; stage 2 (no more stubble grazing)</val>
    <val>{</val>
    <val>  if grazing_stage = GRAZE_STUBBLE</val>
    <val>  {</val>
    <val>     set stubble_days = stubble_days + 1                                        ! Number of days of stubble grazing - This increases every day regardless if stubble grazing or not??</val>
    <val></val>
    <val>     for padd = 1 to no_paddocks</val>
    <val>     {</val>
    <val>        if stubble_present[padd] and crop_in[padd]</val>
    <val>        {</val>
    <val>           set stubble_present[padd] = FALSE</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>     set best_stubble_cover = 0.0                                               ! Threshold stubble cover</val>
    <val>     for padd = 1 to no_paddocks</val>
    <val>     {</val>
    <val>        if stubble_present[padd]</val>
    <val>        {</val>
    <val>           set padd_stubble_cover = summaries.padd_cover[padd]</val>
    <val>           set best_stubble_cover = max(best_stubble_cover, padd_stubble_cover)</val>
    <val>        }</val>
    <val>     }</val>
    <val>     /* TODO: modify this to take into account the presence of annual grass after stubble has been grazed - NH */</val>
    <val>     set best_green_pasture_dm = 0.0</val>
    <val>     for padd = 1 to no_paddocks</val>
    <val>     {</val>
    <val>        if (is_pasture_paddock[padd]  and (summaries.padd_cover[padd] &gt; pasture_cover_threshold))</val>
    <val>        {</val>
    <val>           set best_green_pasture_dm = max( best_green_pasture_dm, sum(pasture_dm_results[padd,1]) )</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>     if (stubble_days &gt;= max_stubble_days) or (best_stubble_cover &lt; stubble_cover_threshold) or (best_green_pasture_dm &gt;= stubble_green_pasture_threshold)</val>
    <val>     {</val>
    <val>        set grazing_stage = GRAZE_GENERAL</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>each 1 Apr</val>
    <val>{</val>
    <val>  set stubble_days = 0</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set stubble_present[padd] = FALSE</val>
    <val>     set spilt_grain_remaining[padd] = 0.0</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Grazing of crops</val>
    <val> * Cultivars within a phase of a rotation system can be nominated</val>
    <val>   as available for dual-purpose grazing</val>
    <val> * Grazing of the crop in a given paddock starts when:</val>
    <val>   - a green mass threshold is exceeded</val>
    <val>   - crop phenology is prior to a threshold</val>
    <val> * Grazing of each crop ends when one or more of the following holds:</val>
    <val>   - green mass falls below a second threshold</val>
    <val>   - crop phenology reaches its theshold</val>
    <val>   - total DSE-days in the paddock reaches (yet another) threshold</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define logical grazed_crop_enough_dm[PADD_COUNT]</val>
    <val>define logical grazed_crop_avail[PADD_COUNT]</val>
    <val>define real    grazed_crop_dse_days[PADD_COUNT]</val>
    <val>define real    grazed_crop_stage</val>
    <val>define real    grazed_crop_green_mass</val>
    <val>define logical do_crop_grazing</val>
    <val>define integer grazed_crop</val>
    <val>define text    crop_model</val>
    <val>define integer rule</val>
    <val></val>
    <val>define real grazed_crop_stages[PADD_COUNT]</val>
    <val>define real grazed_crop_masses[PADD_COUNT]</val>
    <val></val>
    <val>each 1 Jan</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set grazed_crop_avail[padd]        = FALSE</val>
    <val>     set grazed_crop_enough_dm[padd]    = FALSE</val>
    <val>     set grazed_crop_dse_days[padd]     = 0.0</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>from 1 Apr to 30 Nov                                                            ! use this to restrict crop grazing period</val>
    <val>{</val>
    <val>  set do_crop_grazing = FALSE</val>
    <val></val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set grazed_crop = cropping.crop_sown_no[padd]</val>
    <val></val>
    <val>     if grazed_crop &gt; 0</val>
    <val>     {</val>
    <val>        set crop_model               = crop_module[padd,grazed_crop] &amp; '.model'</val>
    <val>        set grazed_crop_stages[padd] = @(crop_model &amp; '.stage')</val>
    <val>        set grazed_crop_masses[padd] = summaries.crop_dm_results[padd,grazed_crop,1]     ! [padd, crop name, green leaf+stem+ear]</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set grazed_crop_stages[padd] = 0.0</val>
    <val>        set grazed_crop_masses[padd] = 0.0</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set grazed_crop_avail[padd] = FALSE</val>
    <val></val>
    <val>     for rule = 1 to length(crop_grazing_info)</val>
    <val>     {</val>
    <val>        if (crop_grazing_info[rule]:rotation = padd_system[padd]) or (crop_grazing_info[rule]:rotation = 'all')</val>
    <val>        {</val>
    <val>           if (crop_grazing_info[rule]:phase = rotations.padd_landuse[padd]) or (crop_grazing_info[rule]:phase = 'all')</val>
    <val>           {</val>
    <val>              if (crop_grazing_info[rule]:cultivar = cropping.cultivar_sown[padd]) or (crop_grazing_info[rule]:cultivar = 'all')</val>
    <val>              {</val>
    <val>                 set grazed_crop = cropping.crop_sown_no[padd]</val>
    <val></val>
    <val>                 set crop_model                  = crop_module[padd,grazed_crop] &amp; '.model'</val>
    <val>                 set grazed_crop_stage           = @(crop_model &amp; '.stage')</val>
    <val>                 set grazed_crop_green_mass      = summaries.crop_dm_results[padd,grazed_crop,1]     ! [padd, crop name, green leaf+stem+ear]</val>
    <val>                 set grazed_crop_enough_dm[padd] = grazed_crop_enough_dm[padd] or (grazed_crop_green_mass &gt; crop_grazing_info[rule]:initial_dm)</val>
    <val>                 set grazed_crop_avail[padd]     = grazed_crop_enough_dm[padd] and (grazed_crop_stage &lt; crop_grazing_info[rule]:final_stage) and (grazed_crop_green_mass &gt; crop_grazing_info[rule]:final_dm) and (grazed_crop_dse_days[padd] &lt; crop_grazing_info[rule]:dse_days)</val>
    <val>                 set do_crop_grazing             = do_crop_grazing or grazed_crop_avail[padd]</val>
    <val>              }</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>  if do_crop_grazing</val>
    <val>  {</val>
    <val>     set grazing_stage     = GRAZE_CROP</val>
    <val>  }</val>
    <val>  else if grazing_stage    = GRAZE_CROP</val>
    <val>  {</val>
    <val>     set grazing_stage     = GRAZE_GENERAL</val>
    <val>     for padd = 1 to no_paddocks</val>
    <val>     {</val>
    <val>        set grazed_crop_enough_dm[padd] = FALSE</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Confinement feeding</val>
    <val> -------------------------------------------------------------------------------</val>
    <val> * Move to confinement feeding when there is no pasture paddock with cover</val>
    <val>   above a threshold.</val>
    <val> * Move out of confinement feeding when the average cover across all pasture</val>
    <val>   paddocks starts to increase substantially</val>
    <val>================================================================================*/</val>
    <val>from start</val>
    <val>{</val>
    <val>  if grazing_stage = GRAZE_GENERAL</val>
    <val>  {</val>
    <val>     set do_confinement_feeding = TRUE</val>
    <val>     for padd = 1 to no_paddocks</val>
    <val>     {</val>
    <val>        if is_pasture_paddock[padd]</val>
    <val>        {</val>
    <val>           set do_confinement_feeding = do_confinement_feeding and (summaries.padd_cover[padd] &lt; pasture_cover_threshold)      !! should this be biomass??</val>
    <val>        }</val>
    <val>     }</val>
    <val>     if do_confinement_feeding</val>
    <val>     {</val>
    <val>        set grazing_stage = GRAZE_CONFINE</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>each 1 Oct                                                                      ! roughly the time of peak cover</val>
    <val>{</val>
    <val>  set lowest_pasture_cover = 1.0                                                 !!!!!!!!!! WHY???????</val>
    <val>}</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>     if summaries.pasture_cover &gt; 0</val>
    <val>     {</val>
    <val>        set lowest_pasture_cover = min( lowest_pasture_cover, summaries.pasture_cover )</val>
    <val>        if (grazing_stage = GRAZE_CONFINE) and (summaries.pasture_cover &gt; lowest_pasture_cover + 0.02)</val>
    <val>        {</val>
    <val>           set grazing_stage = GRAZE_GENERAL</val>
    <val>        }</val>
    <val>     }</val>
    <val>}</val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Pasture grazing</val>
    <val> * At any time of year if there is suitable pasture the animals</val>
    <val>   should always prefer this to confinement in the feedlot</val>
    <val></val>
    <val>   TODO: grazing volunteer grass in summer     !!</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define logical feedlot_stocked</val>
    <val></val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  set pasture_dm_results = summaries.pasture_dm_results</val>
    <val>  set feedlot_stocked = FALSE</val>
    <val>  set i = 1</val>
    <val>  while (feedlot_stocked = FALSE) and (i &lt;= animals.no_groups)</val>
    <val>  {</val>
    <val>     set t = animals.tag_no[i]</val>
    <val></val>
    <val>     if trade_flock_feedlot = TRUE</val>
    <val>     {</val>
    <val>        if (animals.paddock[i] = FEEDLOT) and (t /= TRADE_LAMBS)                ! if animals are currently in feedlot</val>
    <val>        {</val>
    <val>           set feedlot_stocked = TRUE</val>
    <val>        }</val>
    <val>        set i = i + 1</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        if (animals.paddock[i] = FEEDLOT)</val>
    <val>        {</val>
    <val>           set feedlot_stocked = TRUE</val>
    <val>        }</val>
    <val>        set i = i + 1</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>     if (feedlot_stocked = TRUE)</val>
    <val>     {</val>
    <val>        set best_green_pasture_dm = 0.0</val>
    <val>        for padd = 1 to no_paddocks</val>
    <val>        {</val>
    <val>           if is_pasture_paddock[padd] and (summaries.padd_cover[padd] &gt;= pasture_cover_threshold)</val>
    <val>  !          if not crop_in[padd] and (summaries.padd_cover[padd] &gt;= pasture_cover_threshold)</val>
    <val>           {</val>
    <val>              set grazing_stage = GRAZE_GENERAL</val>
    <val>              set best_green_pasture_dm = max( best_green_pasture_dm, sum(pasture_dm_results[padd,1]) )</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>}</val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> These rules allocate animals to paddocks within each stage</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer grazing_period       = 7                                         !! week</val>
    <val>define integer stage_draft_count[3] = [2,1,1]                                   !! each grazing stage has this number of drafts (stubble, general, crop)</val>
    <val></val>
    <val>define text    closed_paddocks[PADD_COUNT+2]                                    ! includes feedlot + trade_feedlot</val>
    <val>define logical one_padd_avail</val>
    <val>define integer draft_no</val>
    <val>define logical draft_group</val>
    <val>define logical draftable_padd[PADD_COUNT]</val>
    <val>define logical draft_to_padd</val>
    <val>define real    best_draftable_cover</val>
    <val>define logical draftable_pasture</val>
    <val>define real    max_spilt_grain</val>
    <val>define logical use_stubble_padd[PADD_COUNT]</val>
    <val>define real    spilt_grain_found</val>
    <val>define logical draftable_stubble</val>
    <val>define logical end_maint_feedlot</val>
    <val>define logical animals_in_paddock</val>
    <val></val>
    <val>define integer grazing_day_count = -1</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  if trade_flock_feedlot = TRUE</val>
    <val>  {</val>
    <val>     if (grazing_stage = GRAZE_CONFINE)</val>
    <val>     {</val>
    <val>        for i = 1 to animals.no_groups</val>
    <val>        {</val>
    <val>           set t = animals.tag_no[i]</val>
    <val>           if (t /= TRADE_LAMBS)</val>
    <val>           {                                                 ! don't shift trade lambs from their feedlot</val>
    <val>              animals.move group=i, paddock=FEEDLOT</val>
    <val>           }</val>
    <val>        }</val>
    <val>        set grazing_day_count = -1                                                 ! Ensures stock are drafted on the first day out of confinement feeding</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        if sum(cropping.crop_sown_flag)                                            ! If any paddock sown, re-allocate animals to paddocks</val>
    <val>        {</val>
    <val>           set grazing_day_count = 0</val>
    <val>        }</val>
    <val>        else</val>
    <val>        {</val>
    <val>           set grazing_day_count = (grazing_day_count + 1) mod grazing_period</val>
    <val>        }</val>
    <val></val>
    <val>        if grazing_day_count = 0</val>
    <val>        {</val>
    <val>           for i = 1 to animals.no_groups                                          ! Make all paddocks unoccupied</val>
    <val>           {</val>
    <val>              set t = animals.tag_no[i]</val>
    <val>              if (t /= TRADE_LAMBS)</val>
    <val>              {                                             ! don't shift trade lambs from their feedlot</val>
    <val>                 animals.move group=i, paddock=''</val>
    <val>              }</val>
    <val>           }</val>
    <val></val>
    <val>           if (grazing_stage = GRAZE_STUBBLE)                                      ! Work out which stubble paddocks should be grazed today</val>
    <val>           {</val>
    <val>              set max_spilt_grain =  max( spilt_grain_remaining )</val>
    <val>              for padd = 1 to no_paddocks</val>
    <val>              {</val>
    <val>                 if spilt_grain_access_rate * max_spilt_grain &lt; 0.05               ! Access all stubbles if animals will get less than 50 g/d of spilt grain</val>
    <val>                 {</val>
    <val>                    set use_stubble_padd[padd] = TRUE</val>
    <val>                 }</val>
    <val>                 else                                                              ! Otherwise, use only the paddocks with most spilt grain</val>
    <val>                 {</val>
    <val>                    set use_stubble_padd[padd] = spilt_grain_remaining[padd] &gt;= 0.5 * max_spilt_grain</val>
    <val>                 }</val>
    <val>              }</val>
    <val>           }</val>
    <val></val>
    <val>           for draft_no = 1 to stage_draft_count[grazing_stage]                     ! Work through a set of separate drafting exercises</val>
    <val>           {</val>
    <val>              for i = 1 to animals.no_groups</val>
    <val>              {</val>
    <val>                 set t = animals.tag_no[i]</val>
    <val></val>
    <val>                 if (grazing_stage = GRAZE_STUBBLE)</val>
    <val>                 {</val>
    <val>                    if (draft_no = 1)</val>
    <val>                    {</val>
    <val>                       set draft_group = (t &gt;= REPLACEMENT_MERINO) and (t &lt;= EWE_MERINO_CULL )  ! merino ewes</val>
    <val>                    }</val>
    <val>                    else</val>
    <val>                    {</val>
    <val>                       set draft_group = ((t &gt;= LAMB_M) and (t &lt;= LAMB_F))                 ! merino lambs</val>
    <val>                    }</val>
    <val>                 }</val>
    <val>                 else if (grazing_stage = GRAZE_CROP)</val>
    <val>                 {</val>
    <val>                    if (draft_no = 1)</val>
    <val>                    {</val>
    <val>                       set draft_group = (t &gt;= REPLACEMENT_MERINO) and (t &lt;= LAMB_F)        ! all breeding flock</val>
    <val>                    }</val>
    <val>                 }</val>
    <val>                 else                                                              ! GRAZE_GENERAL</val>
    <val>                 {</val>
    <val>                    if (t /= TRADE_LAMBS)</val>
    <val>                    {                                         ! don't shift trade lambs from their feedlot</val>
    <val>                       set draft_group = TRUE</val>
    <val>                    }</val>
    <val>                    else</val>
    <val>                    {</val>
    <val>                       set draft_group = FALSE</val>
    <val>                    }</val>
    <val>                 }</val>
    <val></val>
    <val>                 if draft_group</val>
    <val>                 {</val>
    <val>                     animals.prioritise group=i, value=grazing_priorities[t]</val>
    <val>                 }</val>
    <val>                 else</val>
    <val>                 {</val>
    <val>                    animals.prioritise group=i, value=0</val>
    <val>                 }</val>
    <val>              }                                                                    ! [for i]</val>
    <val></val>
    <val>              set one_padd_avail = FALSE</val>
    <val>              for padd = 1 to no_paddocks</val>
    <val>              {</val>
    <val>                 set draftable_pasture   = is_pasture_paddock[padd]   and (summaries.padd_cover[padd] &gt;= pasture_cover_threshold) !and not (crop_in[padd])</val>
    <val>                 set draftable_stubble   = is_cropping_paddock[padd]  and (summaries.padd_cover[padd] &gt;= stubble_cover_threshold) and not (crop_in[padd])</val>
    <val></val>
    <val>                 if (grazing_stage = GRAZE_STUBBLE) and (draft_no = 1)</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = use_stubble_padd[padd]</val>
    <val>                 }</val>
    <val>                 else if (grazing_stage = GRAZE_STUBBLE) and (draft_no = 2)</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = draftable_pasture</val>
    <val>                 }</val>
    <val>                 else if (grazing_stage = GRAZE_GENERAL)</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = draftable_pasture or draftable_stubble</val>
    <val>                 }</val>
    <val>                 else if (grazing_stage = GRAZE_CROP) and (draft_no = 1)</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = grazed_crop_avail[padd]</val>
    <val>                 }</val>
    <val>                 else if (grazing_stage = GRAZE_CROP) and (draft_no = 2)</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = draftable_pasture</val>
    <val>                 }</val>
    <val>                 else</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = FALSE</val>
    <val>                 }</val>
    <val></val>
    <val>! Don't put animals in paddocks belonging to "unused" rotation systems</val>
    <val>                 set draftable_padd[padd] = draftable_padd[padd] and (padd_area[padd] &gt; 0.005*constants.farm_area)</val>
    <val></val>
    <val>                 set one_padd_avail = one_padd_avail or draftable_padd[padd]</val>
    <val></val>
    <val>                 if draftable_padd[padd]</val>
    <val>                 {</val>
    <val>                    set closed_paddocks[padd] = ''</val>
    <val>                 }</val>
    <val>                 else</val>
    <val>                 {</val>
    <val>                    set closed_paddocks[padd] = padd_name[padd]</val>
    <val>                 }</val>
    <val>              }                                                                    ! [for padd]</val>
    <val></val>
    <val>              closed_paddocks[no_paddocks+1] = FEEDLOT</val>
    <val>              closed_paddocks[no_paddocks+2] = TRADE_FEEDLOT</val>
    <val></val>
    <val>! Special case: crops are available for grazing, but no pastures are available</val>
    <val>! In this situation, re-do the drafting logic to put all animals on crops</val>
    <val>              if (grazing_stage = GRAZE_CROP) and (draft_no = 2) and not one_padd_avail</val>
    <val>              {</val>
    <val>                 for i = 1 to animals.no_groups</val>
    <val>                 {</val>
    <val>                    set t = animals.tag_no[i]</val>
    <val>                    if (t /= TRADE_LAMBS)                                          ! don't shift trade lambs from their feedlot</val>
    <val>                    {</val>
    <val>                       animals.move       group=i, paddock=''</val>
    <val>                       animals.prioritise group=i, value=grazing_priorities[ animals.tag_no[i] ]</val>
    <val>                    }</val>
    <val>                 }</val>
    <val>                 for padd = 1 to no_paddocks</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = grazed_crop_avail[padd] and (padd_area[padd] &gt; 0.005*constants.farm_area)</val>
    <val>                    if draftable_padd[padd]</val>
    <val>                    {</val>
    <val>                       set closed_paddocks[padd] = ''</val>
    <val>                    }</val>
    <val>                    else</val>
    <val>                    {</val>
    <val>                       set closed_paddocks[padd] = padd_name[padd]</val>
    <val>                    }</val>
    <val>                 }</val>
    <val>                 animals.draft closed=closed_paddocks</val>
    <val>              }</val>
    <val>              else if one_padd_avail</val>
    <val>              {</val>
    <val>                 animals.draft closed=closed_paddocks</val>
    <val>              }</val>
    <val>              else</val>
    <val>              {</val>
    <val>                 set grazing_stage = GRAZE_CONFINE</val>
    <val>                 for i = 1 to animals.no_groups</val>
    <val>                 {</val>
    <val>                    set t = animals.tag_no[i]</val>
    <val>                    if (t /= TRADE_LAMBS)</val>
    <val>                    {                                       ! don't shift trade lambs from their feedlot</val>
    <val>                       animals.move group=i, paddock=FEEDLOT</val>
    <val>                    }</val>
    <val>                 }</val>
    <val>                 set grazing_day_count = -1</val>
    <val>              }</val>
    <val>           }                                                                       ! [for draft_no]</val>
    <val>        }                                                                          ! if grazing_day_count = 0</val>
    <val>     }                                                                             ! else (not GRAZE_CONFINE)</val>
    <val></val>
    <val>! Make spilt grain available to animals grazing stubbles</val>
    <val>     if (grazing_stage = GRAZE_STUBBLE)</val>
    <val>     {</val>
    <val>        for i = 1 to animals.no_groups</val>
    <val>        {</val>
    <val>           for padd = 1 to no_paddocks</val>
    <val>           {</val>
    <val>              if (animals.paddock[i] = padd_name[padd]) and (spilt_grain_name[padd] /= '') and (spilt_grain_remaining[padd] &gt; 0.0)</val>
    <val>              {</val>
    <val>                 set spilt_grain_found           = min( spilt_grain_max_intake[ spilt_grain_id[padd] ], spilt_grain_access_rate * spilt_grain_remaining[padd] ) * animals.number[i]</val>
    <val>                 set spilt_grain_remaining[padd] = max( 0.0, spilt_grain_remaining[padd] - spilt_grain_found/padd_area[padd] )</val>
    <val>                 supplement.feed supplement=spilt_grain_name[padd], amount=spilt_grain_found, paddock=animals.paddock[i]</val>
    <val>              }</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>     if (maint_feed_location = 'feedlot') and (grazing_stage = GRAZE_CONFINE)     ! they would already be in feedlot if graze_confine</val>
    <val>     {</val>
    <val>        for i = 1 to animals.no_groups</val>
    <val>        {</val>
    <val>           set t = animals.tag_no[i]</val>
    <val>           if (t /= TRADE_LAMBS)</val>
    <val>           {                                              ! don't shift trade lambs from their feedlot</val>
    <val>              animals.move group=i, paddock=FEEDLOT</val>
    <val>           }</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>  ! Output variable - current location of each tag group</val>
    <val>     set paddock_occupied = not_a_paddock</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        set t = animals.tag_no[i]</val>
    <val>        if (t &gt;= REPLACEMENT_MERINO) and (t &lt;= TRADE_LAMBS)</val>
    <val>        {</val>
    <val>           set paddock_occupied[t] = animals.paddock[i]</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>  ! Accumulate dse-days of grazing in each paddock during the dual-purpose cropping stage</val>
    <val>     if (grazing_stage = GRAZE_CROP)</val>
    <val>     {</val>
    <val>        for padd = 1 to no_paddocks</val>
    <val>        {</val>
    <val>           if grazed_crop_avail[padd]</val>
    <val>           {</val>
    <val>              for t = REPLACEMENT_MERINO to (TRADE_LAMBS - 1)</val>
    <val>              if paddock_occupied[t] = padd_name[padd]</val>
    <val>              {</val>
    <val>                 set grazed_crop_dse_days[padd] = grazed_crop_dse_days[padd] + animals.dse_tag[t] + animals.dse_yng_tag[t]</val>
    <val>              }</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>  else if trade_flock_feedlot = false !if trade lambs grazing</val>
    <val>  {</val>
    <val>     if (grazing_stage = GRAZE_CONFINE)</val>
    <val>     {</val>
    <val>        for i = 1 to animals.no_groups</val>
    <val>        {</val>
    <val>           set t = animals.tag_no[i]</val>
    <val>           animals.move group=i, paddock=FEEDLOT</val>
    <val>        }</val>
    <val>        set grazing_day_count = -1                                                 ! Ensures stock are drafted on the first day out of confinement feeding</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        if sum(cropping.crop_sown_flag)                                            ! If any paddock sown, re-allocate animals to paddocks</val>
    <val>        {</val>
    <val>           set grazing_day_count = 0</val>
    <val>        }</val>
    <val>        else</val>
    <val>        {</val>
    <val>           set grazing_day_count = (grazing_day_count + 1) mod grazing_period</val>
    <val>        }</val>
    <val></val>
    <val>        if grazing_day_count = 0</val>
    <val>        {</val>
    <val>           for i = 1 to animals.no_groups                                          ! Make all paddocks unoccupied</val>
    <val>           {</val>
    <val>              set t = animals.tag_no[i]</val>
    <val>              animals.move group=i, paddock=''</val>
    <val>           }</val>
    <val></val>
    <val>           if (grazing_stage = GRAZE_STUBBLE)                                      ! Work out which stubble paddocks should be grazed today</val>
    <val>           {</val>
    <val>              set max_spilt_grain =  max( spilt_grain_remaining )</val>
    <val>              for padd = 1 to no_paddocks</val>
    <val>              {</val>
    <val>                 if spilt_grain_access_rate * max_spilt_grain &lt; 0.05               ! Access all stubbles if animals will get less than 50 g/d of spilt grain</val>
    <val>                 {</val>
    <val>                    set use_stubble_padd[padd] = TRUE</val>
    <val>                 }</val>
    <val>                 else                                                              ! Otherwise, use only the paddocks with most spilt grain</val>
    <val>                 {</val>
    <val>                    set use_stubble_padd[padd] = spilt_grain_remaining[padd] &gt;= 0.5 * max_spilt_grain</val>
    <val>                 }</val>
    <val>              }</val>
    <val>           }</val>
    <val></val>
    <val>           for draft_no = 1 to stage_draft_count[grazing_stage]                     ! Work through a set of separate drafting exercises</val>
    <val>           {</val>
    <val>              for i = 1 to animals.no_groups</val>
    <val>              {</val>
    <val>                 set t = animals.tag_no[i]</val>
    <val></val>
    <val>                 if (grazing_stage = GRAZE_STUBBLE)</val>
    <val>                 {</val>
    <val>                    if (draft_no = 1)</val>
    <val>                    {</val>
    <val>                       set draft_group = (t &gt;= REPLACEMENT_MERINO) and (t &lt;= EWE_MERINO_CULL )  ! merino ewes</val>
    <val>                    }</val>
    <val>                    else</val>
    <val>                    {</val>
    <val>                       set draft_group = ((t &gt;= LAMB_M) and (t &lt;= LAMB_F))                 ! merino lambs</val>
    <val>                    }</val>
    <val>                 }</val>
    <val>                 else if (grazing_stage = GRAZE_CROP)</val>
    <val>                 {</val>
    <val>                    if (draft_no = 1)</val>
    <val>                    {</val>
    <val>                       set draft_group = (t &gt;= REPLACEMENT_MERINO) and (t &lt;= TRADE_LAMBS)        ! all breeding flock</val>
    <val>                    }</val>
    <val>                 }</val>
    <val>                 else                                                              ! GRAZE_GENERAL</val>
    <val>                 {</val>
    <val>                    set draft_group = TRUE</val>
    <val>                 }</val>
    <val></val>
    <val>                 if draft_group</val>
    <val>                 {</val>
    <val>                     animals.prioritise group=i, value=grazing_priorities[t]</val>
    <val>                 }</val>
    <val>                 else</val>
    <val>                 {</val>
    <val>                    animals.prioritise group=i, value=0</val>
    <val>                 }</val>
    <val>              }                                                                    ! [for i]</val>
    <val></val>
    <val>              set one_padd_avail = FALSE</val>
    <val>              for padd = 1 to no_paddocks</val>
    <val>              {</val>
    <val>                 set draftable_pasture   = is_pasture_paddock[padd]   and (summaries.padd_cover[padd] &gt;= pasture_cover_threshold) !and not (crop_in[padd])</val>
    <val>                 set draftable_stubble   = is_cropping_paddock[padd]  and (summaries.padd_cover[padd] &gt;= stubble_cover_threshold) and not (crop_in[padd])</val>
    <val></val>
    <val>                 if (grazing_stage = GRAZE_STUBBLE) and (draft_no = 1)</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = use_stubble_padd[padd]</val>
    <val>                 }</val>
    <val>                 else if (grazing_stage = GRAZE_STUBBLE) and (draft_no = 2)</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = draftable_pasture</val>
    <val>                 }</val>
    <val>                 else if (grazing_stage = GRAZE_GENERAL)</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = draftable_pasture or draftable_stubble</val>
    <val>                 }</val>
    <val>                 else if (grazing_stage = GRAZE_CROP) and (draft_no = 1)</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = grazed_crop_avail[padd]</val>
    <val>                 }</val>
    <val>                 else if (grazing_stage = GRAZE_CROP) and (draft_no = 2)</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = draftable_pasture</val>
    <val>                 }</val>
    <val>                 else</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = FALSE</val>
    <val>                 }</val>
    <val></val>
    <val>! Don't put animals in paddocks belonging to "unused" rotation systems</val>
    <val>                 set draftable_padd[padd] = draftable_padd[padd] and (padd_area[padd] &gt; 0.005*constants.farm_area)</val>
    <val></val>
    <val>                 set one_padd_avail = one_padd_avail or draftable_padd[padd]</val>
    <val></val>
    <val>                 if draftable_padd[padd]</val>
    <val>                 {</val>
    <val>                    set closed_paddocks[padd] = ''</val>
    <val>                 }</val>
    <val>                 else</val>
    <val>                 {</val>
    <val>                    set closed_paddocks[padd] = padd_name[padd]</val>
    <val>                 }</val>
    <val>              }                                                                    ! [for padd]</val>
    <val></val>
    <val>              closed_paddocks[no_paddocks+1] = FEEDLOT</val>
    <val>              closed_paddocks[no_paddocks+2] = TRADE_FEEDLOT</val>
    <val></val>
    <val>! Special case: crops are available for grazing, but no pastures are available</val>
    <val>! In this situation, re-do the drafting logic to put all animals on crops</val>
    <val>              if (grazing_stage = GRAZE_CROP) and (draft_no = 2) and not one_padd_avail</val>
    <val>              {</val>
    <val>                 for i = 1 to animals.no_groups</val>
    <val>                 {</val>
    <val>                    set t = animals.tag_no[i]</val>
    <val>                    animals.move       group=i, paddock=''</val>
    <val>                    animals.prioritise group=i, value=grazing_priorities[ animals.tag_no[i] ]</val>
    <val>                 }</val>
    <val>                 for padd = 1 to no_paddocks</val>
    <val>                 {</val>
    <val>                    set draftable_padd[padd] = grazed_crop_avail[padd] and (padd_area[padd] &gt; 0.005*constants.farm_area)</val>
    <val>                    if draftable_padd[padd]</val>
    <val>                    {</val>
    <val>                       set closed_paddocks[padd] = ''</val>
    <val>                    }</val>
    <val>                    else</val>
    <val>                    {</val>
    <val>                       set closed_paddocks[padd] = padd_name[padd]</val>
    <val>                    }</val>
    <val>                 }</val>
    <val>                 animals.draft closed=closed_paddocks</val>
    <val>              }</val>
    <val>              else if one_padd_avail</val>
    <val>              {</val>
    <val>                 animals.draft closed=closed_paddocks</val>
    <val>              }</val>
    <val>              else</val>
    <val>              {</val>
    <val>                 set grazing_stage = GRAZE_CONFINE</val>
    <val>                 for i = 1 to animals.no_groups</val>
    <val>                 {</val>
    <val>                    set t = animals.tag_no[i]</val>
    <val>                    animals.move group=i, paddock=FEEDLOT</val>
    <val>                 }</val>
    <val>                 set grazing_day_count = -1</val>
    <val>              }</val>
    <val>           }                                                                       ! [for draft_no]</val>
    <val>        }                                                                          ! if grazing_day_count = 0</val>
    <val>     }                                                                             ! else (not GRAZE_CONFINE)</val>
    <val></val>
    <val>! Make spilt grain available to animals grazing stubbles</val>
    <val>     if (grazing_stage = GRAZE_STUBBLE)</val>
    <val>     {</val>
    <val>        for i = 1 to animals.no_groups</val>
    <val>        {</val>
    <val>           for padd = 1 to no_paddocks</val>
    <val>           {</val>
    <val>              if (animals.paddock[i] = padd_name[padd]) and (spilt_grain_name[padd] /= '') and (spilt_grain_remaining[padd] &gt; 0.0)</val>
    <val>              {</val>
    <val>                 set spilt_grain_found           = min( spilt_grain_max_intake[ spilt_grain_id[padd] ], spilt_grain_access_rate * spilt_grain_remaining[padd] ) * animals.number[i]</val>
    <val>                 set spilt_grain_remaining[padd] = max( 0.0, spilt_grain_remaining[padd] - spilt_grain_found/padd_area[padd] )</val>
    <val>                 supplement.feed supplement=spilt_grain_name[padd], amount=spilt_grain_found, paddock=animals.paddock[i]</val>
    <val>              }</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>     if (maint_feed_location = 'feedlot') and (grazing_stage = GRAZE_CONFINE)     ! they would already be in feedlot if graze_confine</val>
    <val>     {</val>
    <val>        for i = 1 to animals.no_groups</val>
    <val>        {</val>
    <val>           set t = animals.tag_no[i]</val>
    <val>           animals.move group=i, paddock=FEEDLOT</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>  ! Output variable - current location of each tag group</val>
    <val>     set paddock_occupied = not_a_paddock</val>
    <val>     for i = 1 to animals.no_groups</val>
    <val>     {</val>
    <val>        set t = animals.tag_no[i]</val>
    <val>        if (t &gt;= REPLACEMENT_MERINO) and (t &lt;= TRADE_LAMBS)</val>
    <val>        {</val>
    <val>           set paddock_occupied[t] = animals.paddock[i]</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>  ! Accumulate dse-days of grazing in each paddock during the dual-purpose cropping stage</val>
    <val>     if (grazing_stage = GRAZE_CROP)</val>
    <val>     {</val>
    <val>        for padd = 1 to no_paddocks</val>
    <val>        {</val>
    <val>           if grazed_crop_avail[padd]</val>
    <val>           {</val>
    <val>              for t = REPLACEMENT_MERINO to (TRADE_LAMBS)</val>
    <val>              if paddock_occupied[t] = padd_name[padd]</val>
    <val>              {</val>
    <val>                 set grazed_crop_dse_days[padd] = grazed_crop_dse_days[padd] + animals.dse_tag[t] + animals.dse_yng_tag[t]</val>
    <val>              }</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
  </init>
  <init name="logfile" kind="string">
    <val></val>
  </init>
