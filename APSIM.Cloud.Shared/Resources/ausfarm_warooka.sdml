    <val>  reset text_01.filename     = file_name_base &amp; str('_daily.txt')</val>
    <val>  reset text_02.filename     = file_name_base &amp; str('_monthly.txt')</val>
    <val>  reset text_03.filename     = file_name_base &amp; str('_yearly.txt')</val>
    <val>  reset text_04.filename     = file_name_base &amp; str('_animal.txt')</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Copy constants across from other modules</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define real    farm_area</val>
    <val>define integer no_rot_systems</val>
    <val>define integer no_paddocks</val>
    <val>define string  padd_name[PADD_COUNT]</val>
    <val>define string  padd_system[PADD_COUNT]</val>
    <val>define integer padd_to_system[PADD_COUNT]</val>
    <val>define integer no_crop_types</val>
    <val>define string  crop_name[CROP_COUNT]</val>
    <val>define integer no_pasture_types</val>
    <val>define string  pasture_module[PADD_COUNT,PAST_COUNT]</val>
    <val>define real    water_summ_depth                                                 ! mm</val>
    <val>define real    nitrogen_summ_depth                                              ! mm</val>
    <val>define string  paddock_has_translators[PADD_COUNT]</val>
    <val>!define logical crop_in[PADD_COUNT]</val>
    <val></val>
    <val>define string  water_model[PADD_COUNT]</val>
    <val>define string  nitrogen_model[PADD_COUNT]</val>
    <val>define string  residue_model[PADD_COUNT]</val>
    <val>define string  crop_model[PADD_COUNT,CROP_COUNT]</val>
    <val></val>
    <val>define integer padd</val>
    <val>define integer crop_no</val>
    <val>define string  model_append</val>
    <val>define text    FEEDLOT</val>
    <val></val>
    <val>define         crop_sowing_info            = [ (rot_system:''; phase:''; landuse:'';  crop:'';      first_day:0; last_day:0;     rain_reqd:0.0; paw_reqd:0.0;      sm_surface:0.0; sow_dry:FALSE;  class:'';      cultivar:''; density:0.0; depth:0.0; spacing:0.0; spray:FALSE) ]</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  set farm_area               = constants.farm_area</val>
    <val>  set no_rot_systems          = constants.no_rot_systems</val>
    <val>  set no_paddocks             = constants.no_paddocks</val>
    <val>  set padd_name               = constants.padd_name</val>
    <val>  set padd_system             = constants.padd_system</val>
    <val>  set padd_to_system          = constants.padd_to_system</val>
    <val>  set no_crop_types           = constants.no_crop_types</val>
    <val>  set crop_name               = constants.crop_name</val>
    <val>  set no_pasture_types        = constants.no_pasture_types</val>
    <val>  set pasture_module          = constants.pasture_module</val>
    <val>  set water_summ_depth        = constants.water_summ_depth</val>
    <val>  set nitrogen_summ_depth     = constants.nitrogen_summ_depth</val>
    <val>  set paddock_has_translators = constants.paddock_has_translators</val>
    <val>  set FEEDLOT                 = constants.FEEDLOT</val>
    <val>!  set crop_in                 = cropping.crop_in</val>
    <val></val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     if constants.paddock_has_translators[padd]</val>
    <val>     {</val>
    <val>        set model_append = '.model.'</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set model_append = '.'</val>
    <val>     }</val>
    <val></val>
    <val>     {</val>
    <val>        set water_model[padd]      = constants.water_module[   padd] &amp; model_append</val>
    <val>        set nitrogen_model[padd]   = constants.nitrogen_module[padd] &amp; model_append</val>
    <val>        set residue_model[padd]    = constants.residue_module[ padd] &amp; model_append</val>
    <val></val>
    <val>        for crop_no = 1 to no_crop_types</val>
    <val>        {</val>
    <val>           if constants.crop_module[padd,crop_no] /= ''</val>
    <val>           {</val>
    <val>              set crop_model[padd,crop_no] = constants.crop_module[padd,crop_no] &amp; model_append</val>
    <val>           }</val>
    <val>           else</val>
    <val>           {</val>
    <val>              set crop_model[padd,crop_no] = ''</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Reporting constants</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer no_cover_levels     = 2</val>
    <val>define real    cover_levels[2]     = [0.50,0.70]</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Output variables</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer report_year</val>
    <val>define integer report_month</val>
    <val></val>
    <val>define integer padd_index[PADD_COUNT]</val>
    <val>define real    system_area[ROT_COUNT]                                                  ! ROT_COUNT?</val>
    <val></val>
    <val>define integer rot_system</val>
    <val>define integer pasture_no</val>
    <val>define integer part</val>
    <val>define integer i</val>
    <val>define integer j</val>
    <val></val>
    <val>each 1 Jan</val>
    <val>{</val>
    <val>  set report_year = year</val>
    <val>}</val>
    <val></val>
    <val>if day_no = 1</val>
    <val>{</val>
    <val>  set report_month = month</val>
    <val>}</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set rot_system = padd_to_system[padd]</val>
    <val>     if length(rot_system_info[rot_system]:sequence) = 1</val>
    <val>     {</val>
    <val>        set padd_index[padd] = 1</val>
    <val>        for j = 1 to padd-1</val>
    <val>        {</val>
    <val>           if padd_system[j] = padd_system[padd]</val>
    <val>           {</val>
    <val>              set padd_index[padd] = padd_index[j] + 1</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set padd_index[padd] = 0</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>  for rot_system = 1 to no_rot_systems</val>
    <val>  {</val>
    <val>     set system_area[rot_system] = 0.0</val>
    <val>  }</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set rot_system = padd_to_system[padd]</val>
    <val>     set system_area[rot_system] = system_area[rot_system]+ padd_area[padd]</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Dry matter variables for each paddock</val>
    <val>================================================================================</val>
    <val> Crops:    1st index: paddock</val>
    <val>           2nd index: crop</val>
    <val>           3rd index: 1=green leaf+stem+ear; 2=grain; 3=dead/stubble</val>
    <val> Pastures: 1st index: paddock</val>
    <val>           2nd index: 1=green mass; 2=dry mass</val>
    <val>           3rd index: species (green), total (dry)</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define real    crop_dm_results[PADD_COUNT,CROP_COUNT,3]                         ! paddocks x crops x crop part</val>
    <val>define real    pasture_dm_results[PADD_COUNT,2,PAST_COUNT]                               ! paddocks x green/dry x pasture</val>
    <val>define real    pasture_green_dm[PADD_COUNT,20]                                  ! paddocks x ??</val>
    <val>define real    pasture_dry_dm[PADD_COUNT,20]                                    ! paddocks x ??</val>
    <val>define real    stock_intake[20,20]                                              ! ?? x ??</val>
    <val>define real    pasture_growth[PADD_COUNT,30,6]                                  ! paddocks x ?? x ??</val>
    <val></val>
    <val></val>
    <val>! ***** Add crop grazing days as an output</val>
    <val></val>
    <val>define string  part_names[6] = ['root','leaf','stem','pod','meal','oil']</val>
    <val>define real    crop_dm[6]                                                       ! Temporary variables</val>
    <val>define integer paddock_count</val>
    <val></val>
    <val>! Re-dimension the biomass summary arrays</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     for crop_no = no_crop_types+1 to no_crop_types                                         !!</val>
    <val>     {</val>
    <val>        set crop_dm_results[padd,crop_no] = []</val>
    <val>     }</val>
    <val>  }</val>
    <val>  for padd = no_paddocks+1 to no_paddocks                                                !!</val>
    <val>  {</val>
    <val>     set crop_dm_results[padd] = [ [] ] }</val>
    <val></val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set pasture_dm_results[padd,1] = pasture_template                          ! One element per pasture species</val>
    <val>     set pasture_dm_results[padd,2] = [0.0]                                     ! One element only</val>
    <val>  }</val>
    <val></val>
    <val>  for padd = no_paddocks+1 to no_paddocks                                                !!</val>
    <val>  {</val>
    <val>     set pasture_dm_results[padd,1] = []</val>
    <val>     set pasture_dm_results[padd,2] = []</val>
    <val>  }</val>
    <val></val>
    <val>  set pasture_green_dm = constants.rotation_template</val>
    <val>  set pasture_dry_dm   = constants.rotation_template</val>
    <val>!  set stock_intake     = constants.rotation_template</val>
    <val></val>
    <val>  for rot_system = 1 to no_rot_systems</val>
    <val>  {</val>
    <val>     if length(rot_system_info[rot_system]:sequence) &gt; 1</val>
    <val>     {</val>
    <val>        set paddock_count = length(rot_system_info[rot_system]:sequence)</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set paddock_count = 0</val>
    <val>        for padd = 1 to no_paddocks</val>
    <val>        {</val>
    <val>           if padd_system[padd] = rot_system_info[rot_system]:name</val>
    <val>           {</val>
    <val>              set paddock_count = paddock_count + 1</val>
    <val>           }</val>
    <val>        }</val>
    <val>     }</val>
    <val>     for i = 1 to paddock_count</val>
    <val>     {</val>
    <val>        set pasture_growth[rot_system,i] = pasture_template</val>
    <val>     }</val>
    <val>     for i = paddock_count+1 to no_paddocks                                              !!</val>
    <val>     {</val>
    <val>        set pasture_growth[rot_system,i] = []</val>
    <val>     }</val>
    <val>  }</val>
    <val>  for rot_system = no_rot_systems+1 to no_rot_systems                                       !!</val>
    <val>  {</val>
    <val>     set pasture_growth[rot_system] = [ [] ]</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     for crop_no = 1 to no_crop_types</val>
    <val>     {</val>
    <val>        if crop_model[padd,crop_no] /= ''</val>
    <val>        {</val>
    <val>           for part = 1 to 6</val>
    <val>           {</val>
    <val>              set crop_dm[part] = @(crop_model[padd,crop_no] &amp; part_names[part] &amp; 'greenwt')</val>
    <val>           }</val>
    <val>           set crop_dm_results[padd,crop_no,1] = 10.0 * (crop_dm[2] + crop_dm[3] + crop_dm[4])</val>
    <val>           set crop_dm_results[padd,crop_no,2] = 10.0 * (crop_dm[5] + crop_dm[6])</val>
    <val></val>
    <val>           for part = 1 to 6</val>
    <val>           {</val>
    <val>              set crop_dm[part] = @(crop_model[padd,crop_no] &amp; part_names[part] &amp; 'senescedwt')</val>
    <val>           }</val>
    <val>           set crop_dm_results[padd,crop_no,3] = 10.0 * (crop_dm[2] + crop_dm[3] + crop_dm[4]) + @(residue_model[padd] &amp; 'surfaceom_wt_' &amp; crop_name[crop_no] )</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>     set pasture_dm_results[padd,2,1] = 0.0</val>
    <val>     for pasture_no = 1 to no_pasture_types</val>
    <val>     {</val>
    <val>        if pasture_module[padd,pasture_no] /= ''</val>
    <val>        {</val>
    <val>           set pasture_dm_results[padd,1,pasture_no] = @(pasture_module[padd,pasture_no] &amp; '.green_dm')</val>
    <val>           set pasture_dm_results[padd,2,1]          = pasture_dm_results[padd,2,1] + @(pasture_module[padd,pasture_no] &amp; '.dry_dm')</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>     set rot_system = padd_to_system[padd]</val>
    <val>     if padd_index[padd] &gt; 0</val>
    <val>     {</val>
    <val>        set i = padd_index[padd]</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set i = padd_year_no[padd]</val>
    <val>     }</val>
    <val></val>
    <val>     set pasture_green_dm[rot_system,i] = sum( pasture_dm_results[padd,1] )</val>
    <val>     set pasture_dry_dm[  rot_system,i] = sum( pasture_dm_results[padd,2] )</val>
    <val></val>
    <val>     for pasture_no = 1 to length(pasture_module[padd])</val>
    <val>     {</val>
    <val>        if pasture_module[padd,pasture_no] /= ''</val>
    <val>        {</val>
    <val>           set pasture_growth[rot_system,i,pasture_no] = @(pasture_module[padd,pasture_no] &amp; '.growth')</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>!    set stock_intake[rot_system,i] = 0.0</val>
    <val>!    for j = 1 to animals.no_groups</val>
    <val>!    { if animals.paddock[j] = padd_name[padd]</val>
    <val>!      { set stock_intake[rot_system,i] = stock_intake[rot_system,i] + animals.number[j]     * animals.past_intake[j]:weight     / padd_area[padd]</val>
    <val>!        set stock_intake[rot_system,i] = stock_intake[rot_system,i] + animals.number_yng[j] * animals.past_intake_yng[j]:weight / padd_area[padd]</val>
    <val>!      }</val>
    <val>!    }</val>
    <val>  }                                                                             ! for padd</val>
    <val>}                                                                               ! from start</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Profile extractable soil water and nitrogen</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer water_summ_weighting[PADD_COUNT,17]                              ! to get total water to water_summ_depth - PADD_COUNT, ??</val>
    <val>define integer nitrogen_summ_weighting[120,17]                                  ! to get total N to nitrogen_summ_depth  - ??, ??</val>
    <val></val>
    <val>define real    padd_ll15_dep[PADD_COUNT,17]                                     ! PADD_COUNT, ??</val>
    <val>define real    padd_esw[PADD_COUNT]</val>
    <val>define real    padd_inorg_n[PADD_COUNT]</val>
    <val>define real    padd_no3[PADD_COUNT]</val>
    <val>define real    total_mineralisation[12]                                         ! PADD_COUNT??</val>
    <val></val>
    <val>define real    layer_data[17]                                                   ! ??</val>
    <val>define real    cumul_depth</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  set padd_esw     = paddock_template</val>
    <val>  set padd_inorg_n = paddock_template</val>
    <val>  set padd_no3     = paddock_template</val>
    <val>  set total_mineralisation = paddock_template</val>
    <val></val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set padd_ll15_dep[padd] = @(water_model[padd] &amp; 'll15_dep')</val>
    <val></val>
    <val>     set layer_data = @(water_model[padd] &amp; 'dlayer')</val>
    <val>     set cumul_depth = 0.0</val>
    <val>     for i = 1 to length(layer_data)</val>
    <val>     {</val>
    <val>        set water_summ_weighting[padd,i]    = max( 0.0, min( (water_summ_depth    - cumul_depth) / layer_data[i], 1.0 ) )</val>
    <val>        set nitrogen_summ_weighting[padd,i] = max( 0.0, min( (nitrogen_summ_depth - cumul_depth) / layer_data[i], 1.0 ) )</val>
    <val>        set cumul_depth = cumul_depth + layer_data[i]</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set layer_data = @(water_model[padd] &amp; 'sw_dep')</val>
    <val></val>
    <val>     set padd_esw[padd] = 0.0</val>
    <val>     for i = 1 to length(layer_data)</val>
    <val>     {</val>
