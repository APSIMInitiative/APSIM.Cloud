      <stem_dead_detach_frac>0.0</stem_dead_detach_frac>
      <stem_sen_detach_frac>0.0</stem_sen_detach_frac>
      <pod_dead_detach_frac>0.0</pod_dead_detach_frac>
      <pod_sen_detach_frac>0.0</pod_sen_detach_frac>
      <meal_dead_detach_frac>0.0</meal_dead_detach_frac>
      <meal_sen_detach_frac>0.0</meal_sen_detach_frac>
      <oil_dead_detach_frac>0.0</oil_dead_detach_frac>
      <oil_sen_detach_frac>0.0</oil_sen_detach_frac>
      <!-- %%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
