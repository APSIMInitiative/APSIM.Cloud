      <y_n_conc_max_stem description="stem maximum N conc.">0.0800  0.0500  0.0400  0.020   0.0150</y_n_conc_max_stem>
      <y_n_conc_crit_stem description="stem critical N conc.">0.0500  0.0300  0.0200  0.0120   0.0070</y_n_conc_crit_stem>
      <y_n_conc_min_stem description="stem minimum N conc.">0.0500  0.020  0.0100  0.0050   0.0020</y_n_conc_min_stem>
      <y_n_conc_max_pod description="pod maximum N conc.">0.0800  0.0600  0.0500  0.0450   0.0250</y_n_conc_max_pod>
      <y_n_conc_crit_pod description="pod critical N conc.">0.0700  0.0500  0.0400  0.0350   0.0200</y_n_conc_crit_pod>
      <y_n_conc_min_pod description="pod minimum N conc.">0.0500  0.040  0.0200  0.010   0.0050</y_n_conc_min_pod>
      <n_conc_max_grain description="grain maximum N conc.">0.045</n_conc_max_grain>
      <n_conc_crit_grain description="grain critical N conc.">0.040</n_conc_crit_grain>
      <n_conc_min_grain description="grain minimum N conc.">0.030</n_conc_min_grain>
      <n_conc_max_root description="root maximum N conc.">0.040</n_conc_max_root>
      <n_conc_crit_root description="root critical N conc.">0.025</n_conc_crit_root>
      <n_conc_min_root description="root minimum N conc.">0.010</n_conc_min_root>
      <y_n_conc_min_meal>0.0     0.0    0.0    0.045   0.045</y_n_conc_min_meal>
