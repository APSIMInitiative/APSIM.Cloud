    <val>define real    winter_crop_area = 0.0</val>
    <val></val>
    <val>define integer SMN_calc_depth_mm</val>
    <val>define real    SMN_limit</val>
    <val></val>
    <val>define integer rainfall_window</val>
    <val>define real    harvest_height</val>
    <val></val>
    <val></val>
    <val>define         crop_sowing_info            = [ (rot_system:''; phase:''; landuse:'';  crop:'';      first_day:0; last_day:0;     rain_reqd:0.0; paw_reqd:0.0;      sm_surface:0.0; sow_dry:FALSE;  class:'';      cultivar:''; density:0.0; depth:0.0; spacing:0.0; spray:FALSE) ]</val>
    <val>define         crop_fert_info              = [ (rot_system:''; phase:''; cultivar:''; das:0; day:0; stage:0.0; zadok:0;   RainDecile:0; growing_season_rain:0;  paw_reqd:0.0; SMN:0.0; base_rate:0.0;  slope:0.0;     base_day:0;        propn_no3:0.0;  propn_nh4:0.0;  propn_urea:0.0) ]</val>
    <val>define         pasture_sowing_info         = [ (rot_system:''; phase:''; pasture:'';  first_day:0;  last_day:0;  rain_reqd:0.0;  sow_dry:FALSE; resow_root_dm:0.0; rate:0.0;       spray:FALSE) ]</val>
    <val>define         pasture_kill_info           = [ (rot_system:''; phase:'';              first_day:0;  last_day:0;  pasture:'';     weed_mass:0.0; killed:0.0) ]</val>
    <val>define real    pasture_fert_scalars[6,50,6]                                     ! what are these?       changed 20 to 50</val>
    <val></val>
    <val>define integer padd</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  set no_paddocks                          = constants.no_paddocks</val>
    <val>  set padd_name                            = constants.padd_name</val>
    <val>  set padd_system                          = constants.padd_system</val>
    <val>  set padd_to_system                       = constants.padd_to_system</val>
    <val>  set crop_name                            = constants.crop_name</val>
    <val>  set water_module                         = constants.water_module</val>
    <val>  set nitrogen_module                      = constants.nitrogen_module</val>
    <val>  set no_crop_types                        = constants.no_crop_types</val>
    <val>  set crop_module                          = constants.crop_module</val>
    <val>  set no_pasture_types                     = constants.no_pasture_types</val>
    <val>  set pasture_module                       = constants.pasture_module</val>
    <val>  set is_weed_species                      = constants.is_weed_species</val>
    <val>  set paddock_has_translators              = constants.paddock_has_translators</val>
    <val>  set pasture_fert_scalars                 = constants.pasture_fert_scalars</val>
    <val>  set rainfall_window                      = constants.rainfall_window</val>
    <val>  set crop_sowing_info                     = constants.crop_sowing_info</val>
    <val>  set crop_fert_info                       = constants.crop_fert_info</val>
    <val>  set pasture_sowing_info                  = constants.pasture_sowing_info</val>
    <val>  set pasture_kill_info                    = constants.pasture_kill_info</val>
    <val>  set crop_sow_water_depth                 = constants.crop_sow_water_depth</val>
    <val></val>
    <val>  set SMN_calc_depth_mm                    = constants.SMN_calc_depth_mm</val>
    <val></val>
    <val>  set winter_start_month                   = constants.winter_start_month</val>
    <val>  set winter_end_month                     = constants.winter_end_month</val>
    <val>  set max_summer_crop_area                 = constants.farm_area * constants.max_summer_crop_propn !+ 0.001</val>
    <val>  set max_winter_crop_area                 = constants.farm_area * constants.max_winter_crop_propn !+ 0.001</val>
    <val></val>
    <val>  set crop_sown                            = constants.paddock_text_template</val>
    <val>  set cultivar_sown                        = constants.paddock_text_template</val>
    <val>  set area_sown                            = constants.paddock_template</val>
    <val>  set padd_n_applied                       = constants.paddock_template</val>
    <val></val>
    <val>  set crop_cut_amount                      = constants.crop_cut_amount</val>
    <val>  set crop_cut_height                      = constants.crop_cut_height</val>
    <val></val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     if paddock_has_translators[padd]</val>
    <val>     {</val>
    <val>        set dlayer_variable[padd] = water_module[padd] &amp; '.model.dlayer'</val>
    <val>        set sw_variable[    padd] = water_module[padd] &amp; '.model.sw'</val>
    <val>     }</val>
    <val>     else</val>
    <val>     {</val>
    <val>        set dlayer_variable[padd] = water_module[padd] &amp; '.dlayer'</val>
    <val>        set sw_variable[    padd] = water_module[padd] &amp; '.sw'</val>
    <val>     }</val>
    <val>     set ll_variable[      padd] = padd_name[padd]    &amp; '.' &amp; constants.rot_system_info[ padd_to_system[padd] ]:reference_plant &amp; '.ll'</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Sow crops on a running rainfall total and/or soil moisture availability</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer paddock_order[PADD_COUNT]</val>
    <val></val>
    <val>define logical crop_in[PADD_COUNT]                                              ! TRUE if a crop has been sown in a paddock</val>
    <val>define integer crop_sown_no[PADD_COUNT]</val>
    <val>define real    cumul_rain                                                       ! n-day rain total</val>
    <val>define real    past_rain[10]                                                    ! Rainfall 1-n days ago   - what does '10' represent?</val>
    <val>define real    avail_soil_water[PADD_COUNT]                                     ! SW above lower limit to a nominated depth (mm)  - what does '30' represent?</val>
    <val>define real    surface_soil_water[PADD_COUNT]                                   ! SW above lower limit in surface layer (mm)</val>
    <val>define real    padd_soil_layer[10]</val>
    <val>define real    padd_soil_water[10]</val>
    <val>define real    padd_soil_limit[10]</val>
    <val>define real    cumul_depth</val>
    <val></val>
    <val>define text    sow_class</val>
    <val>define text    sow_cultivar</val>
    <val>define text    sow_density</val>
    <val>define text    sow_depth</val>
    <val>define text    sow_spacing</val>
    <val></val>
    <val>define integer sow_rule</val>
    <val>define integer days_past</val>
    <val>define logical rot_system_matches</val>
    <val>define logical phase_matches</val>
    <val>define integer crop_no</val>
    <val>define integer pasture_no</val>
    <val>define integer rule</val>
    <val>define integer layer</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set area_sown[padd]     = 0.0</val>
    <val>     set crop_sown[padd]     = '-'</val>
    <val>     set cultivar_sown[padd] = '-'</val>
    <val>  }</val>
    <val>  for crop_no = 1 to CROP_COUNT</val>
    <val>  {</val>
    <val>     set crop_n_applied_month[crop_no] = 0.0</val>
    <val>     set crop_n_applied_year[crop_no] = 0.0</val>
    <val>  }</val>
    <val>  set sum_n_month = 0.0</val>
    <val>  set sum_n_year = 0.0</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>if (day_no = 1) and (month = winter_start_month)</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set area_sown[padd]     = 0.0</val>
    <val>     set crop_sown[padd]     = '-'</val>
    <val>     set cultivar_sown[padd] = '-'</val>
    <val>     set winter_crop_area    = 0.0</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>if (day_no = 1) and (month = 1 + winter_end_month mod 12)</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set summer_crop_area = 0.0</val>
    <val>     if max_summer_crop_area &gt; 0.0</val>
    <val>     {</val>
    <val>        set area_sown[padd]     = 0.0</val>
    <val>        set crop_sown[padd]     = '-'</val>
    <val>        set cultivar_sown[padd] = '-'</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>define logical paddock_ranked[PADD_COUNT]</val>
    <val>define integer rank</val>
    <val>define integer best_padd</val>
    <val>define real    best_avail_water</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  for days_past = rainfall_window to 2 step -1</val>
    <val>  {</val>
    <val>     set past_rain[days_past] = past_rain[days_past-1] }</val>
    <val>     set past_rain[1] = rain</val>
    <val>     set cumul_rain = sum( past_rain )</val>
    <val></val>
    <val>     for padd = 1 to no_paddocks</val>
    <val>     {</val>
    <val>        set padd_soil_layer = @(dlayer_variable[padd])                          ! Calculate  soil water in the profile to a nominated depth</val>
    <val>        set padd_soil_water = @(sw_variable[    padd])</val>
    <val>        set padd_soil_limit = @(ll_variable[    padd])</val>
    <val></val>
    <val>        set surface_soil_water[padd] = padd_soil_layer[1] * max( 0.0, padd_soil_water[1] - padd_soil_limit[1] )</val>
    <val></val>
    <val>        set avail_soil_water[padd] = 0.0</val>
    <val>        set cumul_depth = 0.0</val>
    <val>        for layer = 1 to length(padd_soil_layer)</val>
    <val>        {</val>
    <val>           set avail_soil_water[padd] = avail_soil_water[padd] + max( 0.0, min( crop_sow_water_depth-cumul_depth, padd_soil_layer[layer] ) ) * max( 0.0, padd_soil_water[layer] - padd_soil_limit[layer] )</val>
    <val>           set cumul_depth = cumul_depth + padd_soil_layer[layer]</val>
    <val>        }</val>
    <val>     }</val>
    <val></val>
    <val>     for padd = 1 to no_paddocks</val>
    <val>     {</val>
    <val>        set crop_sown_flag[padd] = FALSE</val>
    <val>     }</val>
    <val></val>
    <val>! Rank paddocks in order of decreasing available SW</val>
    <val></val>
    <val>     for padd = 1 to no_paddocks</val>
    <val>     {</val>
    <val>        set paddock_ranked[padd] = FALSE</val>
    <val>     }</val>
    <val></val>
    <val>     for rank = 1 to no_paddocks</val>
    <val>     {</val>
    <val>        set best_padd = -1</val>
    <val>        set best_avail_water = -9999.9</val>
    <val>        for padd = 1 to no_paddocks</val>
    <val>        {</val>
    <val>           if not paddock_ranked[padd] and (avail_soil_water[padd] &gt; best_avail_water)</val>
    <val>           {</val>
    <val>              set best_padd = padd</val>
    <val>              set best_avail_water = avail_soil_water[padd]</val>
    <val>           }</val>
    <val>        }</val>
    <val>     set paddock_order[ rank     ] = best_padd</val>
    <val>     set paddock_ranked[best_padd] = TRUE</val>
    <val>  }</val>
    <val></val>
    <val>  for rank = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set padd = paddock_order[rank]</val>
    <val></val>
    <val>     if not crop_in[padd]</val>
    <val>     {</val>
    <val>        set sow_rule = 0                                                        ! Is there a sowing rule that applies to this paddock today?</val>
    <val></val>
    <val>        for rule = 1 to length(crop_sowing_info)</val>
    <val>        {</val>
    <val>           set rot_system_matches = (crop_sowing_info[rule]:rot_system = padd_system[padd] ) or (crop_sowing_info[rule]:rot_system = 'all')</val>
    <val>           set phase_matches      = (crop_sowing_info[rule]:phase      = padd_landuse[padd]) or (crop_sowing_info[rule]:phase      = 'all')</val>
    <val>           if (sow_rule = 0) and rot_system_matches and phase_matches</val>
    <val>           {</val>
    <val>              if (day &gt;= crop_sowing_info[rule]:first_day) and (day &lt;= crop_sowing_info[rule]:last_day) and (cumul_rain &gt;= crop_sowing_info[rule]:rain_reqd) and (avail_soil_water[padd] &gt;= crop_sowing_info[rule]:paw_reqd) and (surface_soil_water[padd] &gt;= crop_sowing_info[rule]:sm_surface)</val>
    <val>              {</val>
    <val>                 set sow_rule = rule</val>
    <val>              }</val>
    <val>              else if (day = crop_sowing_info[rule]:last_day) and crop_sowing_info[rule]:sow_dry</val>
    <val>              {</val>
    <val>              set sow_rule = rule</val>
    <val>              }</val>
    <val>           }</val>
    <val>      }</val>
    <val></val>
    <val>! Enforce maximum area constraints</val>
    <val>     if (sow_rule &gt; 0)</val>
    <val>     {</val>
    <val>        if (month &gt;= winter_start_month) and (month &lt;= winter_end_month)</val>
    <val>        {</val>
    <val>           if winter_crop_area + padd_area[padd] &lt;= max_winter_crop_area</val>
    <val>           {</val>
    <val>              set winter_crop_area = winter_crop_area + padd_area[padd]</val>
    <val>           }</val>
    <val>           else</val>
    <val>           {</val>
    <val>              set sow_rule = 0</val>
    <val>           }</val>
    <val>        }</val>
    <val>        else</val>
    <val>        {</val>
    <val>           if summer_crop_area + padd_area[padd] &lt;= max_summer_crop_area</val>
    <val>           {</val>
    <val>              set summer_crop_area = summer_crop_area + padd_area[padd]</val>
    <val>           }</val>
    <val>           else</val>
    <val>           {</val>
    <val>              set sow_rule = 0</val>
    <val>           }</val>
    <val>        }</val>
    <val>      }</val>
    <val></val>
    <val>! Carry out the sowing actions</val>
    <val>     if sow_rule &gt; 0</val>
    <val>     {</val>
    <val>        set crop_no = 1</val>
    <val>        while (crop_no &lt;= no_crop_types) and (crop_sowing_info[sow_rule]:crop /= crop_name[crop_no])</val>
    <val>        {</val>
    <val>           set crop_no = crop_no + 1</val>
    <val>        }</val>
    <val></val>
    <val>! If called for, spray out pastures at sowing</val>
    <val>        if crop_sowing_info[sow_rule]:spray</val>
    <val>        {</val>
    <val>           for pasture_no = 1 to no_pasture_types</val>
    <val>           {</val>
    <val>              if pasture_module[padd,pasture_no] /= ''</val>
    <val>              {</val>
    <val>                 @(pasture_module[padd,pasture_no]).kill propn_herbage=1.0, propn_seed=0.0</val>
    <val>              }</val>
    <val>           }</val>
    <val>        }</val>
    <val></val>
    <val>        set sow_class    = crop_sowing_info[sow_rule]:class</val>
    <val>        set sow_cultivar = crop_sowing_info[sow_rule]:cultivar</val>
    <val>        set sow_density  = crop_sowing_info[sow_rule]:density</val>
    <val>        set sow_depth    = crop_sowing_info[sow_rule]:depth</val>
    <val>        set sow_spacing  = crop_sowing_info[sow_rule]:spacing</val>
    <val></val>
    <val>        @(crop_module[padd,crop_no]).model.sow crop_class=sow_class, cultivar=sow_cultivar, plants=sow_density, sowing_depth=sow_depth, row_spacing=sow_spacing</val>
    <val></val>
    <val>        set crop_sown_flag[padd] = TRUE</val>
    <val>        set crop_in[padd]        = TRUE</val>
    <val>        set area_sown[padd]      = padd_area[padd]</val>
    <val>        set crop_sown[padd]      = crop_sowing_info[sow_rule]:crop</val>
    <val>        set cultivar_sown[padd]  = crop_sowing_info[sow_rule]:cultivar</val>
    <val>        set crop_sown_no[padd]   = crop_no</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val>define text crop_model</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Cut crops for hay</val>
    <val>================================================================================*/</val>
    <val>/*define real crop_stage[PADD_COUNT]</val>
    <val>define real cutting_biomass[PADD_COUNT]</val>
    <val>! a debugging variable</val>
    <val>define text hay_date[PADD_COUNT]</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     for crop_no = 1 to no_crop_types</val>
    <val>     {</val>
    <val>        set crop_cut[padd,crop_no] = FALSE</val>
    <val>     }</val>
    <val>     ! init some debug vars</val>
    <val>     set cutting_biomass[padd]     = 0.0</val>
    <val>     set crop_stage[padd]          = 0.0</val>
    <val>     set hay_date[padd]            = ''</val>
    <val>     set biomass_avail[padd]       = 0.0</val>
    <val>     set biomass_remain[padd]      = 0.0</val>
    <val>     set hay_harvested_today[padd] = 0.0</val>
    <val></val>
    <val>     set crop_no = crop_sown_no[padd]</val>
    <val>     if crop_no &gt; 0</val>
    <val>     {</val>
    <val>        if crop_name[crop_no] = 'fieldpea'</val>
    <val>        {</val>
    <val>           if paddock_has_translators[padd]</val>
    <val>           {</val>
    <val>              set crop_model = crop_module[padd,crop_no] &amp; '.model'</val>
    <val>           }</val>
    <val>           else</val>
    <val>           {</val>
    <val>              set crop_model = crop_module[padd,crop_no]</val>
    <val>           }</val>
    <val></val>
    <val>           if (@(crop_model &amp; '.stagename') /= 'out')</val>
    <val>           {</val>
    <val>              set crop_stage[padd] = @(crop_model &amp; '.stage')                   ! get stage</val>
    <val>              if (crop_stage[padd] &gt;= 6) and (crop_stage[padd] &lt; 7)</val>
    <val>              {</val>
    <val>                 set cutting_biomass[padd] = @(crop_model &amp; '.biomass')         ! get biomass amount</val>
    <val>                 if cutting_biomass[padd] &gt; 1000.0                              ! this can be used to ensure there is enough to cut</val>
    <val>                 {</val>
    <val>                    set crop_cut[padd,crop_no] = TRUE</val>
    <val>                    set hay_date[padd] = dd_mm_yyyy</val>
    <val>                    set biomass_avail[padd] = @(crop_model &amp; '.biomass')</val>
    <val>                    @(crop_model).harvest Plants=0 , Remove=crop_cut_amount , Height=crop_cut_height , Report=''</val>
    <val>                    set biomass_remain[padd] = @(crop_model &amp; '.biomass')</val>
    <val>                    set hay_harvested_today[padd] = biomass_avail[padd] - biomass_remain[padd]</val>
    <val>                    @(crop_model).kill_crop KillFraction=1.0                    ! not sure if this is required</val>
    <val>                    @(crop_model).end_crop</val>
    <val>                    set crop_in[padd]          = FALSE                          ! not treated as a crop paddock any longer</val>
    <val>                    set crop_sown_no[padd]     = 0</val>
    <val>                 }</val>
    <val>              } ! endif suitable cutting growth stage</val>
    <val>           }  ! endif not out</val>
    <val>        } ! endif oats</val>
    <val>     } ! endif a crop sown here</val>
    <val>        if crop_cut[padd,crop_no] = TRUE                                        ! reset crop_sown, cultivar_sown, area_sown</val>
    <val>        {</val>
    <val>              set crop_sown[padd]         = '-'</val>
    <val>              set cultivar_sown[padd]     = '-'</val>
    <val>              set area_sown[padd]         = 0.0</val>
    <val>        }</val>
    <val></val>
    <val>   } ! next paddock</val>
    <val>} */</val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Harvest crops</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     for crop_no = 1 to no_crop_types</val>
