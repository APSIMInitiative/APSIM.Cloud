    <val>define real    biomass_remain[PADD_COUNT]</val>
    <val>define real    grain_harvested_today[PADD_COUNT]                                ! kg/ha</val>
    <val>define real    grain_spilt_today[PADD_COUNT]                                    ! kg/ha</val>
    <val>define string  grain_spilt_name[PADD_COUNT]</val>
    <val></val>
    <val></val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Copy constants across from other modules</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer no_paddocks</val>
    <val>define string  padd_name[PADD_COUNT]</val>
