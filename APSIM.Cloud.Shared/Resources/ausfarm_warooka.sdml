      <root_n_sen_conc>0.010</root_n_sen_conc>
      <leaf_n_sen_conc>0.01</leaf_n_sen_conc>
      <stem_n_sen_conc>0.0</stem_n_sen_conc>
      <pod_n_sen_conc>0.0</pod_n_sen_conc>
      <meal_n_sen_conc>0.0</meal_n_sen_conc>
      <oil_n_sen_conc>0.0</oil_n_sen_conc>
