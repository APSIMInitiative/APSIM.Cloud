  <init name="rules" kind="string" array="T">
    <val>/*==============================================================================</val>
    <val>              Consistent mixed farming system simulations</val>
    <val>================================================================================</val>
    <val></val>
    <val></val>
    <val></val>
    <val>===============================================================================*/</val>
    <val></val>
    <val>define const integer PADD_COUNT          = 11                                   ! from constants</val>
    <val>define const integer ROT_COUNT           = 3</val>
    <val>define const integer CROP_COUNT          = 8</val>
    <val>define const integer PAST_COUNT          = 2</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Names of output files</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define string  file_name_base                                                   ! Simulation-specific part of all file names</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
