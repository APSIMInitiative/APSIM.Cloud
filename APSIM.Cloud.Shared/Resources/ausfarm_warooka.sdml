         sw_avail_sum     = sum_real_array (sw_avail, deepest_layer)
         sw_avail_ratio   = divide (sw_avail_sum, sw_avail_pot_sum, 1.0) !???
         -->
      <x_sw_avail_ratio description="water availability">0.0 0.16</x_sw_avail_ratio>
      <y_swdef_pheno description="stress index for phenology">1.0 1.0</y_swdef_pheno>
