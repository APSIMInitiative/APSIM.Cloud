    <val>define text    do_reset_SOM</val>
    <val>define logical do_reset_seed_pools</val>
    <val>define real    seed_pool_reset_mass</val>
    <val>define real    seed_pool_reset_propn[6,6]                                       ! ??</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  set no_paddocks           = constants.no_paddocks</val>
    <val>  set water_module          = constants.water_module</val>
    <val>  set nitrogen_module       = constants.nitrogen_module</val>
    <val>  set no_pasture_types      = constants.no_pasture_types</val>
    <val>  set pasture_module        = constants.pasture_module</val>
    <val>  set do_reset_SOM          = constants.do_reset_SOM</val>
    <val>  set do_reset_seed_pools   = constants.do_reset_seed_pools</val>
    <val>  set seed_pool_reset_mass  = constants.seed_pool_reset_mass</val>
    <val>  set seed_pool_reset_propn = constants.seed_pool_reset_propn</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Reset soil humus and biomass pools</val>
    <val> -------------------------------------------------------------------------------</val>
    <val> * The strategy is to undo the effects of a "reset" event on the inorganic N</val>
    <val>   and fresh organic matter pools.</val>
    <val> * Note that FOM types named "fom_1", "fom2" and "fom3" must be defined in the</val>
    <val>   initialisation script for the SoilN2 modules</val>
    <val>===============================================================================*/</val>
    <val></val>
    <val>define text    fom_pools[3] = ['fom_1','fom_2','fom_3']</val>
    <val></val>
    <val>define real    layer_thick[15]                                                  ! ??</val>
    <val>define real    fom_c_pools[2,3,15]                                              ! (before &amp; after) x (fom pools) x (layers)</val>
    <val>define real    fom_n_pools[2,3,15]</val>
    <val>define real    nh4_pools[2,15]                                                  ! ??</val>
    <val>define real    no3_pools[2,15]</val>
    <val>define real    urea_pools[2,15]</val>
    <val></val>
    <val>define text    cn_model</val>
    <val>define integer padd</val>
    <val>define integer pool</val>
    <val>define integer layer</val>
    <val></val>
    <val>define delta_fom = [ (thickness:0.0; no3:0.0; nh4:0.0; po4:0.0; pool:[(amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0)]),</val>
    <val>                     (thickness:0.0; no3:0.0; nh4:0.0; po4:0.0; pool:[(amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0)]),</val>
    <val>                     (thickness:0.0; no3:0.0; nh4:0.0; po4:0.0; pool:[(amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0)]),</val>
    <val>                     (thickness:0.0; no3:0.0; nh4:0.0; po4:0.0; pool:[(amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0)]),</val>
    <val>                     (thickness:0.0; no3:0.0; nh4:0.0; po4:0.0; pool:[(amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0)]),</val>
    <val>                     (thickness:0.0; no3:0.0; nh4:0.0; po4:0.0; pool:[(amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0)]),</val>
    <val>                     (thickness:0.0; no3:0.0; nh4:0.0; po4:0.0; pool:[(amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0)]),</val>
    <val>                     (thickness:0.0; no3:0.0; nh4:0.0; po4:0.0; pool:[(amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0)]),</val>
    <val>                     (thickness:0.0; no3:0.0; nh4:0.0; po4:0.0; pool:[(amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0)]),</val>
    <val>                     (thickness:0.0; no3:0.0; nh4:0.0; po4:0.0; pool:[(amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0),</val>
    <val>                                                                      (amount:0.0; C:0.0; N:0.0; P:0.0; AshAlk:0.0)]) ]</val>
    <val></val>
    <val>define padd_reset_oc[30,17]                                                     ! PADD_COUNT, ??</val>
    <val></val>
    <val>each 1 Feb</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     set cn_model = nitrogen_module[padd] &amp; '.model'</val>
    <val></val>
    <val>     if (do_reset_SOM = 'annual') or ((do_reset_SOM = 'sequence') and (padd_year_no[padd] = 1))</val>
    <val>     {</val>
    <val>        set layer_thick = @(water_module[padd] &amp; '.model.dlayer')</val>
    <val></val>
    <val>! Record the pre-reset state of FOM &amp; inorganic N</val>
    <val>        for pool = 1 to 3                                                       ! Store state variables prior to the reset</val>
    <val>        {</val>
    <val>           set fom_c_pools[1,pool] = @(cn_model &amp; '.fom_c_pool' &amp; pool )</val>
    <val>           set fom_n_pools[1,pool] = @(cn_model &amp; '.fom_n_pool' &amp; pool )</val>
    <val>        }</val>
    <val>        set nh4_pools[1]  = @(cn_model &amp; '.nh4'  )</val>
    <val>        set no3_pools[1]  = @(cn_model &amp; '.no3'  )</val>
    <val>        set urea_pools[1] = @(cn_model &amp; '.urea' )</val>
    <val></val>
    <val>! Reset all soil C &amp; N pools back to initial state</val>
    <val>        @(cn_model).reset</val>
    <val></val>
    <val>! Record the altered state of FOM &amp; inorganic N</val>
    <val>        for pool = 1 to 3</val>
    <val>        {</val>
    <val>           set fom_c_pools[2,pool] = @(cn_model &amp; '.fom_c_pool' &amp; pool )</val>
    <val>           set fom_n_pools[2,pool] = @(cn_model &amp; '.fom_n_pool' &amp; pool )</val>
    <val>        }</val>
    <val>        set nh4_pools[2]  = @(cn_model &amp; '.nh4'  )</val>
    <val>        set no3_pools[2]  = @(cn_model &amp; '.no3'  )</val>
    <val>        set urea_pools[2] = @(cn_model &amp; '.urea' )</val>
    <val></val>
    <val>        for layer = 1 to min( length(fom_c_pools[1,1]), length(delta_fom) )</val>
    <val>        {</val>
    <val>           set delta_fom[layer]:thickness   = layer_thick[layer]</val>
    <val>           set delta_fom[layer]:no3         = no3_pools[1,layer] - no3_pools[2,layer]</val>
    <val>           set delta_fom[layer]:nh4         = nh4_pools[1,layer] - nh4_pools[2,layer]</val>
    <val>           set delta_fom[layer]:po4         = 0.0</val>
    <val></val>
    <val>           for pool = 1 to 3</val>
    <val>           {</val>
    <val>              set delta_fom[layer]:pool[pool]:C      = fom_c_pools[1,pool,layer] - fom_c_pools[2,pool,layer]</val>
    <val>              set delta_fom[layer]:pool[pool]:amount = delta_fom[layer]:pool[pool]:C / 0.4</val>
    <val>              set delta_fom[layer]:pool[pool]:N      = fom_n_pools[1,pool,layer] - fom_n_pools[2,pool,layer]</val>
    <val>              set delta_fom[layer]:pool[pool]:P      = 0.0</val>
    <val>              set delta_fom[layer]:pool[pool]:AshAlk = 0.0</val>
    <val>           }</val>
    <val>        }</val>
    <val></val>
    <val>        @(cn_model).incorpfompool layer=delta_fom</val>
    <val>        }                                                                       ! if do_reset_SOM ...</val>
    <val></val>
    <val>! Store SOC immediately after any reset, for use in computing annual OC changes</val>
    <val></val>
    <val>     set padd_reset_oc[padd] = @(cn_model &amp; '.carbon_tot' )</val>
    <val>  }                                                                             ! for padd = 1 to no_paddocks</val>
    <val>}                                                                               !  each 1 Feb</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Reset pasture seed pools at the final crop harvest</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define         old_seeds = (soft_unripe:[0.0]; soft_ripe:[0.0]; hard_unripe:[0.0]; hard_ripe:[0.0])</val>
    <val>define         new_seeds = (soft_unripe:[0.0]; soft_ripe:[0.0]; hard_unripe:[0.0]; hard_ripe:[0.0])</val>
    <val>define real    old_mass</val>
    <val>define real    new_mass</val>
    <val>define integer rot_system</val>
    <val>define integer next_landuse</val>
    <val>define integer pasture_no</val>
    <val></val>
    <val>from start</val>
    <val>{</val>
    <val>  for padd = 1 to no_paddocks</val>
    <val>  {</val>
    <val>     if do_reset_seed_pools and sum(cropping.crop_harvested[padd])</val>
    <val>     {</val>
    <val>        set rot_system = constants.padd_to_system[padd]</val>
    <val></val>
    <val>        set next_landuse = length(constants.rot_system_info[rot_system]:sequence)</val>
    <val>        while (next_landuse &gt; 1) and (constants.rot_system_info[rot_system]:sequence[next_landuse] /= rotations.padd_landuse_next[padd])</val>
    <val>        {</val>
    <val>           set next_landuse = next_landuse - 1</val>
    <val>        }</val>
    <val></val>
    <val>        if not constants.rot_system_info[rot_system]:is_crop[next_landuse]      ! i.e. if we are going out of crop</val>
    <val>        {</val>
    <val>           for pasture_no = 1 to no_pasture_types</val>
    <val>           {</val>
    <val>              if pasture_module[padd,pasture_no] /= ''</val>
    <val>              {</val>
    <val>                 set old_seeds = @(pasture_module[padd,pasture_no] &amp; '.seeds')</val>
    <val>                 set old_mass  = @(pasture_module[padd,pasture_no] &amp; '.seed_dm')</val>
    <val>                 set new_mass  = seed_pool_reset_mass * seed_pool_reset_propn[rot_system,pasture_no]</val>
    <val></val>
    <val>! Compute the new seed pool sizes, keeping proportions the same</val>
    <val>                 set new_seeds:soft_unripe[1] = 0.0</val>
    <val>                 set new_seeds:soft_ripe[1]   = 0.0</val>
    <val>                 set new_seeds:hard_ripe[1]   = 0.0</val>
    <val>                 set new_seeds:hard_unripe[1] = 0.0</val>
    <val></val>
    <val>                 if (old_mass = 0.0)</val>
    <val>                 {</val>
    <val>                    set new_seeds:hard_ripe[1] = new_mass</val>
    <val>                 }</val>
    <val>                 else</val>
    <val>                 {</val>
    <val>                    if length(old_seeds:soft_unripe) &gt;= 1</val>
    <val>                    {</val>
    <val>                       set new_seeds:soft_unripe[1] = old_seeds:soft_unripe[1] * new_mass/old_mass</val>
    <val>                    }</val>
    <val>                    if length(old_seeds:soft_ripe)   &gt;= 1</val>
    <val>                    {</val>
    <val>                       set new_seeds:soft_ripe[1]   = old_seeds:soft_ripe[1]   * new_mass/old_mass</val>
    <val>                    }</val>
    <val>                    if length(old_seeds:hard_unripe) &gt;= 1</val>
    <val>                    {</val>
    <val>                       set new_seeds:hard_unripe[1] = old_seeds:hard_unripe[1] * new_mass/old_mass</val>
    <val>                    }</val>
    <val>                    if length(old_seeds:hard_ripe)   &gt;= 1</val>
    <val>                    {</val>
    <val>                       set new_seeds:hard_ripe[1]   = old_seeds:hard_ripe[1]   * new_mass/old_mass</val>
    <val>                    }</val>
    <val>                 }</val>
    <val></val>
    <val>! Perform the reset</val>
    <val></val>
    <val>                 reset @(pasture_module[padd,pasture_no] &amp; '.seeds') = new_seeds</val>
    <val>              }</val>
    <val>           }                                                                    ! for pasture_no</val>
    <val>        }                                                                       ! if not constants.rot_system_info[]:is_crop[]</val>
    <val>     }                                                                          ! if do_reset_seed_pools and sum(cropping.crop_harvested[])</val>
    <val>  }</val>
    <val>}</val>
  </init>
