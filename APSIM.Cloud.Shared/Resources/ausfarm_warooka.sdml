      <n_conc_max_root description="root critical N conc.">0.01500</n_conc_max_root>
      <n_conc_crit_root description="root maximum N conc.">0.01500</n_conc_crit_root>
      <n_conc_min_root description="root minimum N conc.">0.00795</n_conc_min_root>
      <y_n_conc_min_meal>0.0     0.0     0.0  0.036  0.036</y_n_conc_min_meal>
      <y_n_conc_crit_meal>0.0     0.0     0.0  0.037  0.037</y_n_conc_crit_meal>
