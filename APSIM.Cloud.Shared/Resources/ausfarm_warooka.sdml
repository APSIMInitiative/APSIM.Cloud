      <y_n_conc_min_stem description="stem minimum N conc.">0.0200 0.0150  0.0100  0.0100 0.0100</y_n_conc_min_stem>
      <y_n_conc_crit_stem description="stem critical N conc.">0.0600 0.0450  0.0300  0.0180 0.0170</y_n_conc_crit_stem>
