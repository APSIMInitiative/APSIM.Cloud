  <component name="rotations" active="T" class="Manager">
    <executable name="manager.dll" version="1.1"/>
    <initdata>
      <![CDATA[
<initsection>
  <init name="published_events" array="T">
  </init>
  <init name="subscribed_events" array="T">
  </init>
  <init name="driver_connections" array="T">
  </init>
  <init name="rules" kind="string" array="T">
    <val>/*==============================================================================</val>
    <val>              Consistent mixed farming system simulations</val>
    <val>================================================================================</val>
    <val> Management of the "rotation" (land use sequence) in each paddock</val>
    <val> * Fixed rotation rules</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer padd_year_no[30]                                                 ! what is this? length of sim? number of paddocks?</val>
    <val>define text    padd_landuse[30]</val>
    <val>define text    padd_landuse_next[30]</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Initialisation:</val>
    <val> * initialise the "padd_landuse" and "padd_landuse_next" variables</val>
    <val> * compute the area devoted to each rotation system</val>
    <val> * set up arrays of module names for use in paddock-specific events</val>
    <val> * if pure perennial pastures have been selected, remove all annual pasture</val>
    <val>   in the PPPWWW paddocks</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>define integer system_rot_length[12]                                            ! what is this - phase or number paddocks?</val>
    <val></val>
    <val>define integer padd</val>
    <val>define integer rot_system</val>
    <val>define integer rot_yr</val>
    <val>define integer rot_yr_next</val>
    <val></val>
    <val>on start</val>
    <val>{</val>
    <val>  for rot_system = 1 to constants.no_rot_systems</val>
    <val>  {</val>
    <val>     set system_rot_length[rot_system] = length(rot_system_info[rot_system]:sequence)</val>
    <val></val>
    <val>! Set up each successive paddock in a different phase of the rotation</val>
    <val>     set rot_yr = 0</val>
    <val>     for padd = 1 to constants.no_paddocks</val>
    <val>     {</val>
    <val>        if rot_system = constants.padd_to_system[padd]</val>
    <val>        {</val>
    <val>           set rot_yr             = 1 + rot_yr mod system_rot_length[rot_system]</val>
    <val>           set padd_year_no[padd] = rot_yr</val>
    <val>        }</val>
    <val>     }</val>
    <val>  }</val>
    <val></val>
    <val>  for padd = 1 to constants.no_paddocks</val>
    <val>  {</val>
    <val>     set rot_system              = constants.padd_to_system[padd]</val>
    <val>     set rot_yr                  = padd_year_no[padd]</val>
    <val>     set rot_yr_next             = 1 + rot_yr mod system_rot_length[rot_system]</val>
    <val>     set padd_landuse[     padd] = rot_system_info[rot_system]:sequence[rot_yr]</val>
    <val>     set padd_landuse_next[padd] = rot_system_info[rot_system]:sequence[rot_yr_next]</val>
    <val>  }</val>
    <val>}</val>
    <val></val>
    <val></val>
    <val>/*==============================================================================</val>
    <val> Change the year within each rotation.</val>
    <val> * This logic can be used for any fixed rotation</val>
    <val>================================================================================*/</val>
    <val></val>
    <val>each 1 feb                                                                      ! why reset 1 Feb - why not 1 Jan??</val>
    <val>{</val>
    <val>  for padd = 1 to constants.no_paddocks</val>
    <val>  {</val>
    <val>     set rot_system           = constants.padd_to_system[padd]</val>
    <val>     set rot_yr               = 1 + padd_year_no[padd] mod system_rot_length[rot_system] ! Move to the next year in the rotation system</val>
    <val>     set rot_yr_next          = 1 + rot_yr             mod system_rot_length[rot_system] ! Find the year after next in the rotation system</val>
    <val></val>
    <val>     set padd_year_no[padd]      = rot_yr</val>
    <val>     set padd_landuse[     padd] = rot_system_info[rot_system]:sequence[rot_yr]</val>
