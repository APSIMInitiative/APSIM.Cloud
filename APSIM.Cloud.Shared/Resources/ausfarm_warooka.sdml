      <y_n_conc_min_pod description="pod minimum N conc.">0.0200 0.0150  0.0100  0.0100 0.0100</y_n_conc_min_pod>
      <y_n_conc_crit_pod description="pod critical N conc.">0.0600 0.0450  0.0300  0.0180 0.0170</y_n_conc_crit_pod>
      <y_n_conc_max_pod description="pod maximum N conc.">0.0800 0.0600  0.0350  0.0270 0.0250</y_n_conc_max_pod>
      <n_conc_max_grain description="grain critical N conc.">0.036</n_conc_max_grain>
      <n_conc_crit_grain description="grain maximum N conc.">0.037</n_conc_crit_grain>
